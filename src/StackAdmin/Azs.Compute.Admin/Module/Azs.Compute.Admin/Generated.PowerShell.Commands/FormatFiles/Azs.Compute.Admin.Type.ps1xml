<Types>
    <Type>
        <Name>Microsoft.AzureStack.Management.Compute.Admin.Models.OsDisk</Name>
        <Members>
            <ScriptMethod>
                <Name>ToString</Name>
                <Script>
                  $result = ""
                  if($this.OsType -ne $null) {
                    $result += "type=$($this.OsType) "
                  }
                  if($this.Uri -ne $null) {
                    $result += "uri=$($this.Uri)"
                  }

                  $result -replace " $",""
                </Script>
            </ScriptMethod>
        </Members>
    </Type>
    <Type>
        <Name>Microsoft.AzureStack.Management.Compute.Admin.Models.DataDisk</Name>
        <Members>
            <ScriptMethod>
                <Name>ToString</Name>
                <Script>
                  $result = ""
                  if($this.Lun -ne $null) {
                    $result += "lun=$($this.Lun) "
                  }
                  if($this.Uri -ne $null) {
                    $result += "uri=$($this.Uri)"
                  }

                  $result -replace " $",""
                </Script>
            </ScriptMethod>
        </Members>
    </Type>
</Types>
