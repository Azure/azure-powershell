  "commands": [
    {
      "name": "Get-AzSynapseKustoPoolAttachedDatabaseConfiguration",
      "description": "Returns an attached database configuration.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Synapse/workspaces/{workspaceName}/kustoPools/{kustoPoolName}/attachedDatabaseConfigurations/{attachedDatabaseConfigurationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.synapse/get-azsynapsekustopoolattacheddatabaseconfiguration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AttachedDatabaseConfigurationName <String>",
              "-KustoPoolName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns an attached database configuration.",
          "parameters": [
            {
              "name": "-AttachedDatabaseConfigurationName",
              "value": "[Path.attachedDatabaseConfigurationName]"
            },
            {
              "name": "-KustoPoolName",
              "value": "[Path.kustoPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSynapseKustoPoolAttachedDatabaseConfiguration",
      "description": "Deletes the attached database configuration with the given name.",
