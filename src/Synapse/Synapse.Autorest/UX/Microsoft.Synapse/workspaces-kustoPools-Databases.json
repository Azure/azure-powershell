  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.synapse"
  },
  "commands": [
    {
      "name": "Get-AzSynapseKustoPoolDatabase",
      "description": "Returns a database.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Synapse/workspaces/{workspaceName}/kustoPools/{kustoPoolName}/Databases/{databaseName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.synapse/get-azsynapsekustopooldatabase"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DatabaseName <String>",
              "-KustoPoolName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns a database.",
          "parameters": [
            {
              "name": "-DatabaseName",
              "value": "[Path.databaseName]"
            },
            {
              "name": "-KustoPoolName",
              "value": "[Path.kustoPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSynapseKustoPoolDatabase",
      "description": "Deletes the database with the given name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Synapse/workspaces/{workspaceName}/kustoPools/{kustoPoolName}/Databases/{databaseName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.synapse/remove-azsynapsekustopooldatabase"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DatabaseName <String>",
              "-KustoPoolName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the database with the given name.",
          "parameters": [
            {
              "name": "-DatabaseName",
              "value": "[Path.databaseName]"
            },
            {
              "name": "-KustoPoolName",
              "value": "[Path.kustoPoolName]"
            },
            {
