{
  "resourceType": "locations/deletedConfigurationStores",
  "apiVersion": "2022-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.appconfiguration"
  },
  "commands": [
    {
      "name": "Clear-AzAppConfigurationDeletedStore",
      "description": "Permanently deletes the specified configuration store.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.AppConfiguration/locations/{location}/deletedConfigurationStores/{configStoreName}/purge",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.appconfiguration/clear-azappconfigurationdeletedstore"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Location <String>",
              "-Name <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Permanently deletes the specified configuration store.",
          "parameters": [
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-Name",
              "value": "[Path.configStoreName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzAppConfigurationDeletedStore",
      "description": "Gets a deleted Azure app configuration store.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.AppConfiguration/locations/{location}/deletedConfigurationStores/{configStoreName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.appconfiguration/get-azappconfigurationdeletedstore"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Location <String>",
              "-Name <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a deleted Azure app configuration store.",
          "parameters": [
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-Name",
              "value": "[Path.configStoreName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
