      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagemover/get-azstoragemover"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a Storage Mover resource.",
          "parameters": [
            {
