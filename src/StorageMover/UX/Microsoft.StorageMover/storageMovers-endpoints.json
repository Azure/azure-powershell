      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagemover/get-azstoragemoverendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-StorageMoverName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an Endpoint resource.",
          "parameters": [
            {
              "name": "-Name",
