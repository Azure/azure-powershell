<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PsModuleName>StorageSync</PsModuleName>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)..\..\Az.props" />

  <PropertyGroup>
    <RootNamespace>$(LegacyAssemblyPrefix)$(PsModuleName)</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PreLoadAssemblies Include="$(RepoSrc)lib\Microsoft.Win32.Registry.dll" />
  </ItemGroup>

  
  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Management.StorageSync" Version="2.8.0-preview" />
    <PackageReference Include="System.Management" Version="4.6.0-preview.18571.3" />
	</ItemGroup>
  
  <ItemGroup>
    <EmbeddedResource Include="Config.json" />
  </ItemGroup>

</Project>