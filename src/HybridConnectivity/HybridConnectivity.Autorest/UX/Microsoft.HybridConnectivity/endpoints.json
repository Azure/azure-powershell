      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.hybridconnectivity/get-azhybridconnectivityendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceUri <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the endpoint to the resource.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.endpointName]"
            },
            {
              "name": "-ResourceUri",
              "value": "[Path.resourceUri]"
            }
          ]
        }
      ]
    },
