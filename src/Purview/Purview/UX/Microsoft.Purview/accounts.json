{
  "resourceType": "accounts",
  "apiVersion": "2021-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.purview"
  },
  "commands": [
    {
      "name": "Get-AzPurviewAccount",
      "description": "Get an account",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Purview/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.purview/get-azpurviewaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an account",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPurviewAccount",
      "description": "Deletes an account resource",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Purview/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.purview/remove-azpurviewaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an account resource",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
