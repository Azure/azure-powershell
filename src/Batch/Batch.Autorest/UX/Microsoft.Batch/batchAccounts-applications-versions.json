{
  "resourceType": "batchAccounts/applications/versions",
  "apiVersion": "2024-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.batch"
  },
  "commands": [
    {
      "name": "Get-AzApplicationPackage",
      "description": "Gets information about the specified application package.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Batch/batchAccounts/{accountName}/applications/{applicationName}/versions/{versionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.batch/get-azapplicationpackage"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AccountName <String>",
              "-ApplicationName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VersionName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about the specified application package.",
          "parameters": [
            {
              "name": "-AccountName",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ApplicationName",
              "value": "[Path.applicationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VersionName",
              "value": "[Path.versionName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzApplicationPackage",
      "description": "Deletes an application package record and its associated binary file.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Batch/batchAccounts/{accountName}/applications/{applicationName}/versions/{versionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.batch/remove-azapplicationpackage"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AccountName <String>",
              "-ApplicationName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-VersionName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an application package record and its associated binary file.",
          "parameters": [
            {
              "name": "-AccountName",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ApplicationName",
              "value": "[Path.applicationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VersionName",
              "value": "[Path.versionName]"
            }
          ]
        }
      ]
    }
  ]
}
