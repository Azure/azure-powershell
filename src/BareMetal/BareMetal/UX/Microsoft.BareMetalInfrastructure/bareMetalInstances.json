{
  "resourceType": "bareMetalInstances",
  "apiVersion": "2021-08-09",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.baremetal"
  },
  "commands": [
    {
      "name": "Get-AzBareMetal",
      "description": "Gets an Azure BareMetal instance for the specified subscription, resource group, and instance name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.BareMetalInfrastructure/bareMetalInstances/{azureBareMetalInstanceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.baremetal/get-azbaremetal"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an Azure BareMetal instance for the specified subscription, resource group, and instance name.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.azureBareMetalInstanceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
