              </ListItem>
              <ListItem>
                <PropertyName>IsDefault</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>IsCurrent</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>TenantId</PropertyName>
              </ListItem>
              <ListItem>
                <Label>CurrentStorageAccountName</Label>
                <ScriptBlock>$_.GetAccountName()</ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
