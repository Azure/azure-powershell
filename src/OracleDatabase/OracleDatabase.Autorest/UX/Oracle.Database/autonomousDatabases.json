  },
  "commands": [
    {
      "name": "Get-AzOracleDatabaseAutonomousDatabase",
      "description": "Get a AutonomousDatabase",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Oracle.Database/autonomousDatabases/{autonomousdatabasename}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.oracledatabase/get-azoracledatabaseautonomousdatabase"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a AutonomousDatabase",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.autonomousdatabasename]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Invoke-AzOracleDatabaseShrinkAutonomousDatabase",
      "description": "This operation shrinks the current allocated storage down to the current actual used data storage.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Oracle.Database/autonomousDatabases/{autonomousdatabasename}/shrink",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.oracledatabase/invoke-azoracledatabaseshrinkautonomousdatabase"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Autonomousdatabasename <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "This operation shrinks the current allocated storage down to the current actual used data storage.",
          "parameters": [
            {
              "name": "-Autonomousdatabasename",
              "value": "[Path.autonomousdatabasename]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
