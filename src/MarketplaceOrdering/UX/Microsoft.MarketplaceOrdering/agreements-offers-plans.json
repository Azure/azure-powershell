{
  "resourceType": "agreements/offers/plans",
  "apiVersion": "2021-01-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.marketplaceordering"
  },
  "commands": [
    {
      "name": "Invoke-AzMarketplaceSignTerms",
      "description": "Sign marketplace terms.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.MarketplaceOrdering/agreements/{publisherId}/offers/{offerId}/plans/{planId}/sign",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.marketplaceordering/invoke-azmarketplacesignterms"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Product <String>",
              "-Publisher <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Sign marketplace terms.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.planId]"
            },
            {
              "name": "-Product",
              "value": "[Path.offerId]"
            },
            {
              "name": "-Publisher",
              "value": "[Path.publisherId]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzMarketplaceTerms",
      "description": "Cancel marketplace terms.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.MarketplaceOrdering/agreements/{publisherId}/offers/{offerId}/plans/{planId}/cancel",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.marketplaceordering/stop-azmarketplaceterms"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Product <String>",
              "-Publisher <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Cancel marketplace terms.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.planId]"
            },
            {
              "name": "-Product",
              "value": "[Path.offerId]"
            },
            {
              "name": "-Publisher",
              "value": "[Path.publisherId]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
