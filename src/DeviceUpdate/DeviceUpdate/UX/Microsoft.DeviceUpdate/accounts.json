{
  "resourceType": "accounts",
  "apiVersion": "2022-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.deviceupdate"
  },
  "commands": [
    {
      "name": "Get-AzDeviceUpdateAccount",
      "description": "Returns account details for the given account name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DeviceUpdate/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.deviceupdate/get-azdeviceupdateaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns account details for the given account name.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDeviceUpdateAccount",
      "description": "Deletes account.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DeviceUpdate/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.deviceupdate/remove-azdeviceupdateaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes account.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
