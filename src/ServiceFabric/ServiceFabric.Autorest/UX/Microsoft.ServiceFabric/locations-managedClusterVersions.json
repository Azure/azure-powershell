{
  "resourceType": "locations/managedClusterVersions",
  "apiVersion": "2025-03-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.servicefabric"
  },
  "commands": [
    {
      "name": "Get-AzServiceFabricManagedClusterVersion",
      "description": "Gets information about an available Service Fabric cluster code version by environment.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ServiceFabric/locations/{location}/managedClusterVersions/{clusterVersion}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.servicefabric/get-azservicefabricmanagedclusterversion"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterVersion <String>",
              "-Location <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about an available Service Fabric managed cluster code version.",
          "parameters": [
            {
              "name": "-ClusterVersion",
              "value": "[Path.clusterVersion]"
            },
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
