{
  "resourceType": "jobs",
  "apiVersion": "2021-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.databox"
  },
  "commands": [
    {
      "name": "Get-AzDataBoxJob",
      "description": "Gets information about the specified job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataBox/jobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.databox/get-azdataboxjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about the specified job.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataBoxJob",
      "description": "Deletes a job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataBox/jobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.databox/remove-azdataboxjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a job.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzDataBoxJob",
      "description": "CancelJob.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataBox/jobs/{jobName}/cancel",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.databox/stop-azdataboxjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "CancelJob.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
