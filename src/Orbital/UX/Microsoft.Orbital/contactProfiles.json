      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.orbital/get-azorbitalcontactprofile"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the specified contact Profile in a specified resource group.",
          "parameters": [
            {
              "value": "[Path.contactProfileName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
