{
  "resourceType": "spacecrafts",
  "apiVersion": "2022-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.orbital"
  },
  "commands": [
    {
      "name": "Get-AzOrbitalSpacecraft",
      "description": "Gets the specified spacecraft in a specified resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Orbital/spacecrafts/{spacecraftName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.orbital/get-azorbitalspacecraft"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the specified spacecraft in a specified resource group.",
          "parameters": [
            {
              "value": "[Path.spacecraftName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzOrbitalSpacecraft",
      "description": "Deletes a specified spacecraft resource.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Orbital/spacecrafts/{spacecraftName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.orbital/remove-azorbitalspacecraft"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a specified spacecraft resource.",
          "parameters": [
            {
              "value": "[Path.spacecraftName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
