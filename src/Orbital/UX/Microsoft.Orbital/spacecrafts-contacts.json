{
  "resourceType": "spacecrafts/contacts",
  "apiVersion": "2022-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.orbital"
  },
  "commands": [
    {
      "name": "Get-AzOrbitalSpacecraftContact",
      "description": "Gets the specified contact in a specified resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Orbital/spacecrafts/{spacecraftName}/contacts/{contactName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.orbital/get-azorbitalspacecraftcontact"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-SpacecraftName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the specified contact in a specified resource group.",
          "parameters": [
            {
              "value": "[Path.contactName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.spacecraftName]",
              "name": "-SpacecraftName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzOrbitalSpacecraftContact",
      "description": "Deletes a specified contact.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Orbital/spacecrafts/{spacecraftName}/contacts/{contactName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.orbital/remove-azorbitalspacecraftcontact"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-SpacecraftName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a specified contact.",
          "parameters": [
            {
              "value": "[Path.contactName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.spacecraftName]",
              "name": "-SpacecraftName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
