      "description": "Retrieve a valid AAD token for an Endpoint using AMLToken-based authentication.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearningServices/workspaces/{workspaceName}/onlineEndpoints/{endpointName}/token",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices/get-azmlworkspaceonlineendpointtoken"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Retrieve a valid AAD token for an Endpoint using AMLToken-based authentication.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.endpointName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
