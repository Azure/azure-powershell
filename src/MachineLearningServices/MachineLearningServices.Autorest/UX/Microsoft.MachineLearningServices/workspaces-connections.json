{
  "resourceType": "workspaces/connections",
  "apiVersion": "2024-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices"
  },
  "commands": [
    {
      "name": "Get-AzMLWorkspaceConnection",
      "description": "Get a new workspace connection",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearningServices/workspaces/{workspaceName}/connections/{connectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices/get-azmlworkspaceconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a new workspace connection",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.connectionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzMLWorkspaceConnection",
      "description": "Remove a new workspace connection",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearningServices/workspaces/{workspaceName}/connections/{connectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices/remove-azmlworkspaceconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Remove a new workspace connection",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.connectionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    }
  ]
}
