  },
  "commands": [
    {
      "name": "Get-AzMLWorkspaceBatchDeployment",
      "description": "Gets a batch inference deployment by id.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearningServices/workspaces/{workspaceName}/batchEndpoints/{endpointName}/deployments/{deploymentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices/get-azmlworkspacebatchdeployment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EndpointName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a batch inference deployment by id.",
          "parameters": [
            {
              "name": "-EndpointName",
              "value": "[Path.endpointName]"
            },
            {
              "name": "-Name",
              "value": "[Path.deploymentName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzMLWorkspaceBatchDeployment",
      "description": "Delete Batch Inference deployment (asynchronous).",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MachineLearningServices/workspaces/{workspaceName}/batchEndpoints/{endpointName}/deployments/{deploymentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.machinelearningservices/remove-azmlworkspacebatchdeployment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EndpointName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete Batch Inference deployment (asynchronous).",
          "parameters": [
            {
              "name": "-EndpointName",
              "value": "[Path.endpointName]"
            },
            {
              "name": "-Name",
              "value": "[Path.deploymentName]"
            },
            {
              "name": "-ResourceGroupName",
