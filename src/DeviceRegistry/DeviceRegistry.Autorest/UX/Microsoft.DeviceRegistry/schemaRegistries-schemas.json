          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.schemaName]"
            },
            {
              "name": "-RegistryName",
              "value": "[Path.schemaRegistryName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
