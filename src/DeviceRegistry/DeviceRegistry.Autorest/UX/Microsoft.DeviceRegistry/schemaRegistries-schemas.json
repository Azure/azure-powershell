        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-RegistryName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
