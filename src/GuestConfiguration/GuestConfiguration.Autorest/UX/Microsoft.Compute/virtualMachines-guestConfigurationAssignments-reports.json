{
  "resourceType": "virtualMachines/guestConfigurationAssignments/reports",
  "apiVersion": "2022-01-25",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.guestconfiguration"
  },
  "commands": [
    {
      "name": "Get-AzGuestConfigurationAssignmentReport",
      "description": "Get a report for the guest configuration assignment, by reportId.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/virtualMachines/{vmName}/providers/Microsoft.GuestConfiguration/guestConfigurationAssignments/{guestConfigurationAssignmentName}/reports/{reportId}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.guestconfiguration/get-azguestconfigurationassignmentreport"
        },
        "parameterSets": [
          {
            "parameters": [
              "-GuestConfigurationAssignmentName <String>",
              "-ReportId <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VMName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a report for the guest configuration assignment, by reportId.",
          "parameters": [
            {
              "name": "-GuestConfigurationAssignmentName",
              "value": "[Path.guestConfigurationAssignmentName]"
            },
            {
              "name": "-ReportId",
              "value": "[Path.reportId]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VMName",
              "value": "[Path.vmName]"
            }
          ]
        }
      ]
    }
  ]
}
