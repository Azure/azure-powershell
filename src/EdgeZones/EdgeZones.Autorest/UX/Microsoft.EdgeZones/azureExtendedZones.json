{
  "resourceType": "azureExtendedZones",
  "apiVersion": "2024-04-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/edgezones"
  },
  "commands": [
    {
      "name": "Get-AzureExtendedZone",
      "description": "Get an Azure Extended Zone for a subscription",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.EdgeZones/azureExtendedZones/{azureExtendedZoneName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/edgezones/get-azureextendedzone"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AzureExtendedZoneName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an Azure Extended Zone for a subscription",
          "parameters": [
            {
              "name": "-AzureExtendedZoneName",
              "value": "[Path.azureExtendedZoneName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Register-AzureExtendedZone",
      "description": "Registers a subscription for an Azure Extended Zone",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.EdgeZones/azureExtendedZones/{azureExtendedZoneName}/register",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/edgezones/register-azureextendedzone"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AzureExtendedZoneName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Registers a subscription for an Azure Extended Zone",
          "parameters": [
            {
              "name": "-AzureExtendedZoneName",
              "value": "[Path.azureExtendedZoneName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Unregister-AzureExtendedZone",
      "description": "Unregisters a subscription for an Azure Extended Zone",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.EdgeZones/azureExtendedZones/{azureExtendedZoneName}/unregister",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/edgezones/unregister-azureextendedzone"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AzureExtendedZoneName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Unregisters a subscription for an Azure Extended Zone",
          "parameters": [
            {
              "name": "-AzureExtendedZoneName",
              "value": "[Path.azureExtendedZoneName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
