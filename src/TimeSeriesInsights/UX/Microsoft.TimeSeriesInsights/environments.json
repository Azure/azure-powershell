{
  "resourceType": "environments",
  "apiVersion": "2020-05-15",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights"
  },
  "commands": [
    {
      "name": "Get-AzTimeSeriesInsightsEnvironment",
      "description": "Gets the environment with the specified name in the specified subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights/get-aztimeseriesinsightsenvironment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the environment with the specified name in the specified subscription and resource group.",
          "parameters": [
            {
              "value": "[Path.environmentName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzTimeSeriesInsightsEnvironment",
      "description": "Deletes the environment with the specified name in the specified subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights/remove-aztimeseriesinsightsenvironment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the environment with the specified name in the specified subscription and resource group.",
          "parameters": [
            {
              "value": "[Path.environmentName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
