{
  "resourceType": "environments/eventsources",
  "apiVersion": "2020-05-15",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights"
  },
  "commands": [
    {
      "name": "Get-AzTimeSeriesInsightsEventSource",
      "description": "Gets the event source with the specified name in the specified environment.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}/eventsources/{eventSourceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights/get-aztimeseriesinsightseventsource"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvironmentName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the event source with the specified name in the specified environment.",
          "parameters": [
            {
              "name": "-EnvironmentName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.eventSourceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzTimeSeriesInsightsEventSource",
      "description": "Deletes the event source with the specified name in the specified subscription, resource group, and environment",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.TimeSeriesInsights/environments/{environmentName}/eventsources/{eventSourceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.timeseriesinsights/remove-aztimeseriesinsightseventsource"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvironmentName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the event source with the specified name in the specified subscription, resource group, and environment",
          "parameters": [
            {
              "name": "-EnvironmentName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.eventSourceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
