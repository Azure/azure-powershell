{
  "resourceType": "dashboards",
  "apiVersion": "2019-01-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.portal"
  },
  "commands": [
    {
      "name": "Get-AzPortalDashboard",
      "description": "Gets the Dashboard.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Portal/dashboards/{dashboardName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.portal/get-azportaldashboard"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the Dashboard.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dashboardName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPortalDashboard",
      "description": "Deletes the Dashboard.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Portal/dashboards/{dashboardName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.portal/remove-azportaldashboard"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the Dashboard.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dashboardName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
