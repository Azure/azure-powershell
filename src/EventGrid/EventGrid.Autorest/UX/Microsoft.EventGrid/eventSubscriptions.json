        },
        "parameterSets": [
          {
            "parameters": [
              "-EventSubscriptionName <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get all delivery attributes for an event subscription.",
          "parameters": [
            {
              "name": "-EventSubscriptionName",
              "value": "[Path.eventSubscriptionName]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
