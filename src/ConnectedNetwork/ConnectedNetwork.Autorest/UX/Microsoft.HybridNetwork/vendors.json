{
  "resourceType": "vendors",
  "apiVersion": "2021-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.connectednetwork"
  },
  "commands": [
    {
      "name": "Get-AzConnectedNetworkVendor",
      "description": "Gets information about the specified vendor.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.HybridNetwork/vendors/{vendorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.connectednetwork/get-azconnectednetworkvendor"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about the specified vendor.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.vendorName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzConnectedNetworkVendor",
      "description": "Deletes the specified vendor.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.HybridNetwork/vendors/{vendorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.connectednetwork/remove-azconnectednetworkvendor"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the specified vendor.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.vendorName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
