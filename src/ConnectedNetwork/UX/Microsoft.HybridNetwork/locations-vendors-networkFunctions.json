      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.HybridNetwork/locations/{locationName}/vendors/{vendorName}/networkFunctions/{serviceKey}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.connectednetwork/get-azconnectednetworkvendorfunction"
        },
        "parameterSets": [
          {
            "parameters": [
              "-LocationName <String>",
              "-ServiceKey <String>",
              "[-SubscriptionId <String[]>]",
              "-VendorName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about the specified vendor network function.",
          "parameters": [
            {
              "name": "-LocationName",
              "value": "[Path.locationName]"
            },
            {
              "name": "-ServiceKey",
              "value": "[Path.serviceKey]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VendorName",
              "value": "[Path.vendorName]"
            }
          ]
        }
      ]
    }
  ]
}
