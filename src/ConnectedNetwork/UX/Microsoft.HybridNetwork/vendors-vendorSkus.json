      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.connectednetwork/get-azconnectednetworkvendorsku"
        },
        "parameterSets": [
          {
            "parameters": [
              "-SkuName <String>",
              "[-SubscriptionId <String[]>]",
              "-VendorName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets information about the specified sku.",
          "parameters": [
            {
              "value": "[Path.skuName]",
              "name": "-SkuName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            },
            {
              "value": "[Path.vendorName]",
              "name": "-VendorName"
            }
          ]
        }
      ]
    },
    {
