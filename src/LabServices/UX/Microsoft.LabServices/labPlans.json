{
  "resourceType": "labPlans",
  "apiVersion": "2021-10-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.labservices"
  },
  "commands": [
    {
      "name": "Get-AzLabServicesLabPlan",
      "description": "Retrieves the properties of a Lab Plan.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labPlans/{labPlanName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/get-azlabserviceslabplan"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Retrieves the properties of a Lab Plan.",
          "parameters": [
            {
              "value": "[Path.labPlanName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzLabServicesLabPlan",
      "description": "Operation to delete a Lab Plan resource.\nDeleting a lab plan does not delete labs associated with a lab plan, nor does it delete shared images added to a gallery via the lab plan permission container.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labPlans/{labPlanName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/remove-azlabserviceslabplan"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Operation to delete a Lab Plan resource. Deleting a lab plan does not delete labs associated with a lab plan, nor does it delete shared images added to a gallery via the lab plan permission container.",
          "parameters": [
            {
              "value": "[Path.labPlanName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Save-AzLabServicesLabPlanImage",
      "description": "Saves an image from a lab VM to the attached shared image gallery.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labPlans/{labPlanName}/saveImage",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/save-azlabserviceslabplanimage"
        },
        "parameterSets": [
          {
            "parameters": [
              "-LabPlanName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Saves an image from a lab VM to the attached shared image gallery.",
          "parameters": [
            {
              "value": "[Path.labPlanName]",
              "name": "-LabPlanName"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
