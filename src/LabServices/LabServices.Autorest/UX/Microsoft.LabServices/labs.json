{
  "resourceType": "labs",
  "apiVersion": "2021-10-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.labservices"
  },
  "commands": [
    {
      "name": "Get-AzLabServicesLab",
      "description": "API to get labs.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labs/{labName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/get-azlabserviceslab"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the properties of a lab resource.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.labName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Publish-AzLabServicesLab",
      "description": "Publish or re-publish a lab.\nThis will create or update all lab resources, such as virtual machines.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labs/{labName}/publish",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/publish-azlabserviceslab"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Publish or re-publish a lab. This will create or update all lab resources, such as virtual machines.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.labName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzLabServicesLab",
      "description": "Operation to delete a lab resource.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labs/{labName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/remove-azlabserviceslab"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Operation to delete a lab resource.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.labName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Sync-AzLabServicesLabUser",
      "description": "Action used to manually kick off an AAD group sync job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.LabServices/labs/{labName}/syncGroup",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.labservices/sync-azlabserviceslabuser"
        },
        "parameterSets": [
          {
            "parameters": [
              "-LabName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Action used to manually kick off an AAD group sync job.",
          "parameters": [
            {
              "name": "-LabName",
              "value": "[Path.labName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
