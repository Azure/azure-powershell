{
  "resourceType": "elasticSans/volumegroups",
  "apiVersion": "2024-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.elasticsan"
  },
  "commands": [
    {
      "name": "Get-AzElasticSanVolumeGroup",
      "description": "Get either a list of all volume groups from an Elastic SAN or get a single volume group from an Elastic SAN.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ElasticSan/elasticSans/{elasticSanName}/volumegroups/{volumeGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.elasticsan/get-azelasticsanvolumegroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ElasticSanName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get either a list of all volume groups from an Elastic SAN or get a single volume group from an Elastic SAN.",
          "parameters": [
            {
              "name": "-ElasticSanName",
              "value": "[Path.elasticSanName]"
            },
            {
              "name": "-Name",
              "value": "[Path.volumeGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzElasticSanVolumeGroup",
      "description": "Delete an VolumeGroup.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ElasticSan/elasticSans/{elasticSanName}/volumegroups/{volumeGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.elasticsan/remove-azelasticsanvolumegroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ElasticSanName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete an VolumeGroup.",
          "parameters": [
            {
              "name": "-ElasticSanName",
              "value": "[Path.elasticSanName]"
            },
            {
              "name": "-Name",
              "value": "[Path.volumeGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
