{
  "resourceType": "attestationProviders",
  "apiVersion": "2020-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.attestation"
  },
  "commands": [
    {
      "name": "Get-AzAttestationProvider",
      "description": "Get the status of Attestation Provider.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Attestation/attestationProviders/{providerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.attestation/get-azattestationprovider"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the status of Attestation Provider.",
          "parameters": [
            {
              "value": "[Path.providerName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzAttestationProvider",
      "description": "Delete Attestation Service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Attestation/attestationProviders/{providerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.attestation/remove-azattestationprovider"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete Attestation Service.",
          "parameters": [
            {
              "value": "[Path.providerName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
