PS C:\&gt; $DatabaseMap.Name = "contosodb"
PS C:\&gt; $DatabaseMap.TargetEdition = "Standard"
PS C:\&gt; $DatabaseMap.TargetServiceLevelObjective = "S0"
PS C:\&gt; Start-AzureRmSqlServerUpgrade -ResourceGroupName "resourcegroup01" -ServerName "server01" -ServerVersion 12.0 -ScheduleUpgradeAfterUtcDateTime $ScheduleTime -DatabaseCollection ($DatabaseMap)</Cmd>
