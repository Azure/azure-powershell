<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <ViewDefinitions>
    <View>
      <Name>Microsoft.Azure.Commands.Batch.BatchAccountContext</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Batch.BatchAccountContext</TypeName>
      </ViewSelectedBy>
      <CustomControl>
        <CustomEntries>
          <CustomEntry>
            <CustomItem>
              <Frame>
                <CustomItem>
                  <ExpressionBinding>
                    <!-- Omit the keys from display by default. -->
                    <!-- Also omit Tags, since there's a separate TagsTable property specifically for display purposes. -->
                    <ScriptBlock>
                      $str = $_ | Select-Object -Property * -ExcludeProperty PrimaryAccountKey,SecondaryAccountKey,Tags | Format-List -Property * | Out-String
                      $str
                    </ScriptBlock>
                  </ExpressionBinding>
                </CustomItem>
              </Frame>
            </CustomItem>
          </CustomEntry>
        </CustomEntries>
      </CustomControl>
    </View>
    <View>
      <Name>Microsoft.Azure.Commands.Batch.Models.PSPoolNodeCounts</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Batch.Models.PSPoolNodeCounts</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>PoolId</Label>
            <Width>30</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Dedicated</Label>
            <Width>60</Width>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>LowPriority</Label>
            <Width>60</Width>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>PoolId</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>[Microsoft.Azure.Commands.Batch.Models.PSNodeCounts]::FormatObject($_.Dedicated)</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>[Microsoft.Azure.Commands.Batch.Models.PSNodeCounts]::FormatObject($_.LowPriority)</ScriptBlock>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>    </View>
  </ViewDefinitions>
</Configuration>
