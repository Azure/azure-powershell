        <CommandParameterSetInfo2 Name="__AllParameterSets" Params="ApiManagement Location Sku Capacity VirtualNetwork Verbose Debug ErrorAction WarningAction InformationAction ErrorVariable WarningVariable InformationVariable OutVariable OutBuffer PipelineVariable" />
