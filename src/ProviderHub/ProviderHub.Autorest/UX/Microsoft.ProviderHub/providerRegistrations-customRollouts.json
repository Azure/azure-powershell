{
  "resourceType": "providerRegistrations/customRollouts",
  "apiVersion": "2020-11-20",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.providerhub"
  },
  "commands": [
    {
      "name": "Get-AzProviderHubCustomRollout",
      "description": "Gets the custom rollout details.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/customRollouts/{rolloutName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/get-azproviderhubcustomrollout"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProviderNamespace <String>",
              "-RolloutName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the custom rollout details.",
          "parameters": [
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-RolloutName",
              "value": "[Path.rolloutName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
