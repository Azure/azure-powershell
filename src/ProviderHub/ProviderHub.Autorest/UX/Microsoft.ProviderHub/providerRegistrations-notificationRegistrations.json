{
  "resourceType": "providerRegistrations/notificationRegistrations",
  "apiVersion": "2020-11-20",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.providerhub"
  },
  "commands": [
    {
      "name": "Get-AzProviderHubNotificationRegistration",
      "description": "Gets the notification registration details.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/notificationRegistrations/{notificationRegistrationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/get-azproviderhubnotificationregistration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProviderNamespace <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the notification registration details.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.notificationRegistrationName]"
            },
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzProviderHubNotificationRegistration",
      "description": "Deletes a notification registration.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/notificationRegistrations/{notificationRegistrationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/remove-azproviderhubnotificationregistration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProviderNamespace <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a notification registration.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.notificationRegistrationName]"
            },
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
