{
  "resourceType": "providerRegistrations/defaultRollouts",
  "apiVersion": "2020-11-20",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.providerhub"
  },
  "commands": [
    {
      "name": "Get-AzProviderHubDefaultRollout",
      "description": "Gets the default rollout details.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/defaultRollouts/{rolloutName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/get-azproviderhubdefaultrollout"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProviderNamespace <String>",
              "-RolloutName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the default rollout details.",
          "parameters": [
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-RolloutName",
              "value": "[Path.rolloutName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzProviderHubDefaultRollout",
      "description": "Deletes the rollout resource.\nRollout must be in terminal state.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/defaultRollouts/{rolloutName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/remove-azproviderhubdefaultrollout"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProviderNamespace <String>",
              "-RolloutName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the rollout resource. Rollout must be in terminal state.",
          "parameters": [
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-RolloutName",
              "value": "[Path.rolloutName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzProviderHubDefaultRollout",
      "description": "Stops or cancels the rollout, if in progress.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.ProviderHub/providerRegistrations/{providerNamespace}/defaultRollouts/{rolloutName}/stop",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.providerhub/stop-azproviderhubdefaultrollout"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProviderNamespace <String>",
              "-RolloutName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Stops or cancels the rollout, if in progress.",
          "parameters": [
            {
              "name": "-ProviderNamespace",
              "value": "[Path.providerNamespace]"
            },
            {
              "name": "-RolloutName",
              "value": "[Path.rolloutName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
