{
  "resourceType": "imageTemplates",
  "apiVersion": "2022-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.imagebuilder"
  },
  "commands": [
    {
      "name": "Get-AzImageBuilderTemplate",
      "description": "Get information about a virtual machine image template",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.VirtualMachineImages/imageTemplates/{imageTemplateName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.imagebuilder/get-azimagebuildertemplate"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get information about a virtual machine image template",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.imageTemplateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzImageBuilderTemplate",
      "description": "Delete a virtual machine image template",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.VirtualMachineImages/imageTemplates/{imageTemplateName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.imagebuilder/remove-azimagebuildertemplate"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a virtual machine image template",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.imageTemplateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Start-AzImageBuilderTemplate",
      "description": "Create artifacts from a existing image template",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.VirtualMachineImages/imageTemplates/{imageTemplateName}/run",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.imagebuilder/start-azimagebuildertemplate"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Create artifacts from a existing image template",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.imageTemplateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzImageBuilderTemplate",
      "description": "Cancel the long running image build based on the image template",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.VirtualMachineImages/imageTemplates/{imageTemplateName}/cancel",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.imagebuilder/stop-azimagebuildertemplate"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Cancel the long running image build based on the image template",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.imageTemplateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
