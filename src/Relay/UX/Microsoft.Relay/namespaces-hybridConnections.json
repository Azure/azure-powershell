{
  "resourceType": "namespaces/hybridConnections",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.relay"
  },
  "commands": [
    {
      "name": "Get-AzRelayHybridConnection",
      "description": "Returns the description for the specified hybrid connection.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/hybridConnections/{hybridConnectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/get-azrelayhybridconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Namespace <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the description for the specified hybrid connection.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.hybridConnectionName]"
            },
            {
              "name": "-Namespace",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzRelayHybridConnection",
      "description": "Deletes a hybrid connection.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/hybridConnections/{hybridConnectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/remove-azrelayhybridconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Namespace <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a hybrid connection.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.hybridConnectionName]"
            },
            {
              "name": "-Namespace",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
