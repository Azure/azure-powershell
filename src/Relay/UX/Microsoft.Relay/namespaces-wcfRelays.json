{
  "resourceType": "namespaces/wcfRelays",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.relay"
  },
  "commands": [
    {
      "name": "Get-AzWcfRelay",
      "description": "Returns the description for the specified WCF relay.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/wcfRelays/{relayName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/get-azwcfrelay"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Namespace <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the description for the specified WCF relay.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.relayName]"
            },
            {
              "name": "-Namespace",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzWcfRelay",
      "description": "Deletes a WCF relay.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/wcfRelays/{relayName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/remove-azwcfrelay"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Namespace <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a WCF relay.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.relayName]"
            },
            {
              "name": "-Namespace",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
