{
  "resourceType": "namespaces",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.relay"
  },
  "commands": [
    {
      "name": "Get-AzRelayNamespaceNetworkRuleSet",
      "description": "Gets NetworkRuleSet for a Namespace.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}/networkRuleSets/default",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/get-azrelaynamespacenetworkruleset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-NamespaceName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets NetworkRuleSet for a Namespace.",
          "parameters": [
            {
              "name": "-NamespaceName",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzRelayNamespace",
      "description": "Returns the description for the specified namespace.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.relay/get-azrelaynamespace"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the description for the specified namespace.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.namespaceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzRelayNamespace",
      "description": "Deletes an existing namespace.\nThis operation also removes all associated resources under the namespace.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Relay/namespaces/{namespaceName}",
      "help": {
        "learnMore": {
