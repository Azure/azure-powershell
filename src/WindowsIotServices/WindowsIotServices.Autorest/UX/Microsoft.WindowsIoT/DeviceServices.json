{
  "resourceType": "DeviceServices",
  "apiVersion": "2019-06-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.windowsiotservices"
  },
  "commands": [
    {
      "name": "Get-AzWindowsIotServicesDevice",
      "description": "Get the non-security related metadata of a Windows IoT Device Service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.WindowsIoT/DeviceServices/{deviceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.windowsiotservices/get-azwindowsiotservicesdevice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the non-security related metadata of a Windows IoT Device Service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.deviceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzWindowsIotServicesDevice",
      "description": "Delete a Windows IoT Device Service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.WindowsIoT/DeviceServices/{deviceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.windowsiotservices/remove-azwindowsiotservicesdevice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Windows IoT Device Service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.deviceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
