{
  "resourceType": "healthBots",
  "apiVersion": "2020-12-08",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.healthbot"
  },
  "commands": [
    {
      "name": "Get-AzHealthBot",
      "description": "Get a HealthBot.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthBot/healthBots/{botName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthbot/get-azhealthbot"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a HealthBot.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.botName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzHealthBot",
      "description": "Delete a HealthBot.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthBot/healthBots/{botName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthbot/remove-azhealthbot"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a HealthBot.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.botName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
