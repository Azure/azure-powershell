{
  "resourceType": "sqlVirtualMachines",
  "apiVersion": "2022-08-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.sqlvirtualmachine"
  },
  "commands": [
    {
      "name": "Invoke-AzRedeploySqlVM",
      "description": "Uninstalls and reinstalls the SQL IaaS Extension.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.SqlVirtualMachine/sqlVirtualMachines/{sqlVirtualMachineName}/redeploy",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.sqlvirtualmachine/invoke-azredeploysqlvm"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "-SqlVirtualMachineName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Uninstalls and reinstalls the SQL IaaS Extension.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SqlVirtualMachineName",
              "value": "[Path.sqlVirtualMachineName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSqlVM",
      "description": "Deletes a SQL virtual machine.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.SqlVirtualMachine/sqlVirtualMachines/{sqlVirtualMachineName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.sqlvirtualmachine/remove-azsqlvm"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a SQL virtual machine.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.sqlVirtualMachineName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Start-AzSqlVMAssessment",
      "description": "Starts SQL best practices Assessment on SQL virtual machine.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.SqlVirtualMachine/sqlVirtualMachines/{sqlVirtualMachineName}/startAssessment",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.sqlvirtualmachine/start-azsqlvmassessment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "-SqlVirtualMachineName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Starts SQL best practices Assessment on SQL virtual machine.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SqlVirtualMachineName",
              "value": "[Path.sqlVirtualMachineName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
