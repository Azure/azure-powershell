{
  "resourceType": "ledgers",
  "apiVersion": "2022-05-13",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.confidentialledger"
  },
  "commands": [
    {
      "name": "Get-AzConfidentialLedger",
      "description": "Retrieves the properties of a Confidential Ledger.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ConfidentialLedger/ledgers/{ledgerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.confidentialledger/get-azconfidentialledger"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Retrieves the properties of a Confidential Ledger.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.ledgerName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzConfidentialLedger",
      "description": "Deletes an existing Confidential Ledger.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ConfidentialLedger/ledgers/{ledgerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.confidentialledger/remove-azconfidentialledger"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing Confidential Ledger.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.ledgerName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
