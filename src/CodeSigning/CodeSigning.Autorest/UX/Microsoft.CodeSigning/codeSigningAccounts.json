{
  "resourceType": "codeSigningAccounts",
  "apiVersion": "2024-09-30-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/codesigning"
  },
  "commands": [
    {
      "name": "Get-CodeSigningAccount",
      "description": "Get a trusted Signing Account.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.CodeSigning/codeSigningAccounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/codesigning/get-codesigningaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AccountName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a trusted Signing Account.",
          "parameters": [
            {
              "name": "-AccountName",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-CodeSigningAccount",
      "description": "Delete a trusted signing account.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.CodeSigning/codeSigningAccounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/codesigning/remove-codesigningaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AccountName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a trusted signing account.",
          "parameters": [
            {
              "name": "-AccountName",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
