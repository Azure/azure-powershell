{
  "resourceType": "fluidRelayServers/fluidRelayContainers",
  "apiVersion": "2022-06-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay"
  },
  "commands": [
    {
      "name": "Get-AzFluidRelayContainer",
      "description": "Get a Fluid Relay container.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroup}/providers/Microsoft.FluidRelay/fluidRelayServers/{fluidRelayServerName}/fluidRelayContainers/{fluidRelayContainerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay/get-azfluidrelaycontainer"
        },
        "parameterSets": [
          {
            "parameters": [
              "-FluidRelayServerName <String>",
              "-Name <String>",
              "-ResourceGroup <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Fluid Relay container.",
          "parameters": [
            {
              "name": "-FluidRelayServerName",
              "value": "[Path.fluidRelayServerName]"
            },
            {
              "name": "-Name",
              "value": "[Path.fluidRelayContainerName]"
            },
            {
              "name": "-ResourceGroup",
              "value": "[Path.resourceGroup]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzFluidRelayContainer",
      "description": "Delete a Fluid Relay container.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroup}/providers/Microsoft.FluidRelay/fluidRelayServers/{fluidRelayServerName}/fluidRelayContainers/{fluidRelayContainerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay/remove-azfluidrelaycontainer"
        },
        "parameterSets": [
          {
            "parameters": [
              "-FluidRelayServerName <String>",
              "-Name <String>",
              "-ResourceGroup <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Fluid Relay container.",
          "parameters": [
            {
              "name": "-FluidRelayServerName",
              "value": "[Path.fluidRelayServerName]"
            },
            {
              "name": "-Name",
              "value": "[Path.fluidRelayContainerName]"
            },
            {
              "name": "-ResourceGroup",
              "value": "[Path.resourceGroup]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
