  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay"
  },
  "commands": [
    {
      "name": "Get-AzFluidRelayServer",
      "description": "Get a Fluid Relay server.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroup}/providers/Microsoft.FluidRelay/fluidRelayServers/{fluidRelayServerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay/get-azfluidrelayserver"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroup <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Fluid Relay server.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.fluidRelayServerName]"
            },
            {
              "name": "-ResourceGroup",
              "value": "[Path.resourceGroup]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzFluidRelayServer",
      "description": "Delete a Fluid Relay server.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroup}/providers/Microsoft.FluidRelay/fluidRelayServers/{fluidRelayServerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.fluidrelay/remove-azfluidrelayserver"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroup <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Fluid Relay server.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.fluidRelayServerName]"
            },
            {
              "name": "-ResourceGroup",
              "value": "[Path.resourceGroup]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
