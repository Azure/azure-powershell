  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.mdp"
  },
  "commands": [
    {
      "name": "Get-AzMdpPool",
      "description": "Get a Pool",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DevOpsInfrastructure/pools/{poolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.mdp/get-azmdppool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Pool",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.poolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzMdpPool",
      "description": "Delete a Pool",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DevOpsInfrastructure/pools/{poolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.mdp/remove-azmdppool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Pool",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.poolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
