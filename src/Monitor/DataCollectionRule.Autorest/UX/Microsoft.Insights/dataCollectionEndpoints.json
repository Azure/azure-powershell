{
  "resourceType": "dataCollectionEndpoints",
  "apiVersion": "2022-06-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.monitor"
  },
  "commands": [
    {
      "name": "Get-AzDataCollectionEndpoint",
      "description": "Returns the specified data collection endpoint.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints/{dataCollectionEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/get-azdatacollectionendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the specified data collection endpoint.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dataCollectionEndpointName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataCollectionEndpoint",
      "description": "Deletes a data collection endpoint.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionEndpoints/{dataCollectionEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/remove-azdatacollectionendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a data collection endpoint.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dataCollectionEndpointName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
