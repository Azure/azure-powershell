{
  "resourceType": "dataCollectionRules",
  "apiVersion": "2022-06-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.monitor"
  },
  "commands": [
    {
      "name": "Get-AzDataCollectionRule",
      "description": "Returns the specified data collection rule.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionRules/{dataCollectionRuleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/get-azdatacollectionrule"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the specified data collection rule.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dataCollectionRuleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataCollectionRule",
      "description": "Deletes a data collection rule.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/dataCollectionRules/{dataCollectionRuleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/remove-azdatacollectionrule"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a data collection rule.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dataCollectionRuleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
