  "commands": [
    {
      "name": "Get-AzActionGroup",
      "description": "Get an action group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Insights/actionGroups/{actionGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/get-azactiongroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an action group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.actionGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
