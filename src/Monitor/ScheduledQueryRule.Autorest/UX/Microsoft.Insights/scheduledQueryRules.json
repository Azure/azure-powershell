          "url": "https://learn.microsoft.com/powershell/module/az.monitor/get-azscheduledqueryrule"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
