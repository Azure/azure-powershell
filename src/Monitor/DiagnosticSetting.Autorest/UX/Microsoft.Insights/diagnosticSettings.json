      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.monitor/get-azdiagnosticsetting"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceId <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the active diagnostic settings for the specified resource.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.name]"
            },
            {
              "name": "-ResourceId",
              "value": "[Path.resourceUri]"
            }
          ]
        }
      ]
    },
