  "commands": [
    {
      "name": "Get-AzQumuloFileSystem",
      "description": "Get a file system resource",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Qumulo.Storage/fileSystems/{fileSystemName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.qumulo/get-azqumulofilesystem"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a file system resource",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.fileSystemName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzQumuloFileSystem",
      "description": "Delete a file system resource",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Qumulo.Storage/fileSystems/{fileSystemName}",
      "help": {
