{
  "resourceType": "backupVaults/backupPolicies",
  "apiVersion": "2024-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dataprotection"
  },
  "commands": [
    {
      "name": "Get-AzDataProtectionBackupPolicy",
      "description": "Gets a backup policy belonging to a backup vault",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupPolicies/{backupPolicyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/get-azdataprotectionbackuppolicy"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VaultName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a backup policy belonging to a backup vault",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.backupPolicyName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VaultName",
              "value": "[Path.vaultName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataProtectionBackupPolicy",
      "description": "Deletes a backup policy belonging to a backup vault",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupPolicies/{backupPolicyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/remove-azdataprotectionbackuppolicy"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-VaultName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a backup policy belonging to a backup vault",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.backupPolicyName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VaultName",
              "value": "[Path.vaultName]"
            }
          ]
        }
      ]
    }
  ]
}
