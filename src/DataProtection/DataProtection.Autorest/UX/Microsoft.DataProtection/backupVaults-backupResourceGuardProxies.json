{
  "resourceType": "backupVaults/backupResourceGuardProxies",
  "apiVersion": "2024-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dataprotection"
  },
  "commands": [
    {
      "name": "Get-AzDataProtectionResourceGuardMapping",
      "description": "Returns the ResourceGuardProxy object associated with the vault, and that matches the name in the request",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupResourceGuardProxies/{resourceGuardProxyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/get-azdataprotectionresourceguardmapping"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VaultName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns the ResourceGuardProxy object associated with the vault, and that matches the name in the request",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VaultName",
              "value": "[Path.vaultName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataProtectionResourceGuardMapping",
      "description": "Deletes the ResourceGuardProxy",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupResourceGuardProxies/{resourceGuardProxyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/remove-azdataprotectionresourceguardmapping"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-VaultName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the ResourceGuardProxy",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VaultName",
              "value": "[Path.vaultName]"
            }
          ]
        }
      ]
    }
  ]
}
