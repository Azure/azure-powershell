{
  "resourceType": "resourceGuards",
  "apiVersion": "2024-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dataprotection"
  },
  "commands": [
    {
      "name": "Get-AzDataProtectionResourceGuard",
      "description": "Returns a ResourceGuard belonging to a resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/resourceGuards/{resourceGuardsName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/get-azdataprotectionresourceguard"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns a ResourceGuard belonging to a resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.resourceGuardsName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDataProtectionResourceGuard",
      "description": "Deletes a ResourceGuard resource from the resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/resourceGuards/{resourceGuardsName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/remove-azdataprotectionresourceguard"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a ResourceGuard resource from the resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.resourceGuardsName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
