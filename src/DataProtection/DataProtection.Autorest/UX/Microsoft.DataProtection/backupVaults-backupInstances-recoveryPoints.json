{
  "resourceType": "backupVaults/backupInstances/recoveryPoints",
  "apiVersion": "2024-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dataprotection"
  },
  "commands": [
    {
      "name": "Get-AzDataProtectionRecoveryPoint",
      "description": "Gets a Recovery Point using recoveryPointId for a Datasource.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DataProtection/backupVaults/{vaultName}/backupInstances/{backupInstanceName}/recoveryPoints/{recoveryPointId}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dataprotection/get-azdataprotectionrecoverypoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-BackupInstanceName <String>",
              "-Id <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VaultName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a Recovery Point using recoveryPointId for a Datasource.",
          "parameters": [
            {
              "name": "-BackupInstanceName",
              "value": "[Path.backupInstanceName]"
            },
            {
              "name": "-Id",
              "value": "[Path.recoveryPointId]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VaultName",
              "value": "[Path.vaultName]"
            }
          ]
        }
      ]
    }
  ]
}
