          }
        ]
      },
      "examples": [
        {
          "description": "Delete Database Migration Service.",
          "parameters": [
            {
              "value": "[Path.sqlMigrationServiceName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
