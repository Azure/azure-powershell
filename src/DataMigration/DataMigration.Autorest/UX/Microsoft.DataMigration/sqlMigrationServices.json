      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.datamigration/remove-azdatamigrationsqlservicenode"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "-SqlMigrationServiceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the integration runtime node.",
          "parameters": [
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.sqlMigrationServiceName]",
              "name": "-SqlMigrationServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
