{
  "resourceType": "diagnostics",
  "apiVersion": "2023-09-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.selfhelp"
  },
  "commands": [
    {
      "name": "Get-AzSelfHelpDiagnostic",
      "description": "Get the diagnostics using the 'diagnosticsResourceName' you chose while creating the diagnostic.",
      "path": "/{scope}/providers/Microsoft.Help/diagnostics/{diagnosticsResourceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.selfhelp/get-azselfhelpdiagnostic"
        },
        "parameterSets": [
          {
            "parameters": [
              "-SResourceName <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the diagnostics using the 'diagnosticsResourceName' you chose while creating the diagnostic.",
          "parameters": [
            {
              "name": "-SResourceName",
              "value": "[Path.diagnosticsResourceName]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
    }
  ]
}
