{
  "resourceType": "NetworkExperimentProfiles/Experiments",
  "apiVersion": "2019-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.frontdoor"
  },
  "commands": [
    {
      "name": "Get-AzFrontDoorExperiment",
      "description": "Gets an Experiment by ExperimentName",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/NetworkExperimentProfiles/{profileName}/Experiments/{experimentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.frontdoor/get-azfrontdoorexperiment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an Experiment by ExperimentName",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.experimentName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzFrontDoorExperiment",
      "description": "Deletes an Experiment",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/NetworkExperimentProfiles/{profileName}/Experiments/{experimentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.frontdoor/remove-azfrontdoorexperiment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an Experiment",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.experimentName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
