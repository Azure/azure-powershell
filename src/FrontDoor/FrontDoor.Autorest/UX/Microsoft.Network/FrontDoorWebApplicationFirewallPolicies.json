      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.frontdoor/get-azfrontdoorwafpolicy"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Retrieve protection policy with specified name within a resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.policyName]"
            },
