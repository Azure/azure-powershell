{
  "resourceType": "userAssignedIdentities/federatedIdentityCredentials",
  "apiVersion": "2023-01-31",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.managedserviceidentity"
  },
  "commands": [
    {
      "name": "Get-AzFederatedIdentityCredential",
      "description": "Gets the federated identity credential.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedIdentity/userAssignedIdentities/{resourceName}/federatedIdentityCredentials/{federatedIdentityCredentialResourceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managedserviceidentity/get-azfederatedidentitycredential"
        },
        "parameterSets": [
          {
            "parameters": [
              "-IdentityName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the federated identity credential.",
          "parameters": [
            {
              "name": "-IdentityName",
              "value": "[Path.resourceName]"
            },
            {
              "name": "-Name",
              "value": "[Path.federatedIdentityCredentialResourceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzFederatedIdentityCredential",
      "description": "Deletes the federated identity credential.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedIdentity/userAssignedIdentities/{resourceName}/federatedIdentityCredentials/{federatedIdentityCredentialResourceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managedserviceidentity/remove-azfederatedidentitycredential"
        },
        "parameterSets": [
          {
            "parameters": [
              "-IdentityName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the federated identity credential.",
          "parameters": [
            {
              "name": "-IdentityName",
              "value": "[Path.resourceName]"
            },
            {
              "name": "-Name",
              "value": "[Path.federatedIdentityCredentialResourceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
