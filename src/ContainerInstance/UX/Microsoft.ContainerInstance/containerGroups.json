{
  "resourceType": "containerGroups",
  "apiVersion": "2022-10-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.containerinstance"
  },
  "commands": [
    {
      "name": "Get-AzContainerGroup",
      "description": "Gets the properties of the specified container group in the specified subscription and resource group.\nThe operation returns the properties of each container group including containers, image registry credentials, restart policy, IP address type, OS type, state, and volumes.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/get-azcontainergroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the properties of the specified container group in the specified subscription and resource group. The operation returns the properties of each container group including containers, image registry credentials, restart policy, IP address type, OS type, state, and volumes.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzContainerInstanceContainerGroupOutboundNetworkDependencyEndpoint",
      "description": "Gets all the network dependencies for this container group to allow complete control of network setting and configuration.\nFor container groups, this will always be an empty list.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}/outboundNetworkDependenciesEndpoints",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/get-azcontainerinstancecontainergroupoutboundnetworkdependencyendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ContainerGroupName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets all the network dependencies for this container group to allow complete control of network setting and configuration. For container groups, this will always be an empty list.",
          "parameters": [
            {
              "name": "-ContainerGroupName",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerGroup",
      "description": "Delete the specified container group in the specified subscription and resource group.\nThe operation does not delete other resources provided by the user, such as volumes.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/remove-azcontainergroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the specified container group in the specified subscription and resource group. The operation does not delete other resources provided by the user, such as volumes.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Restart-AzContainerGroup",
      "description": "Restarts all containers in a container group in place.\nIf container image has updates, new image will be downloaded.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}/restart",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/restart-azcontainergroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Restarts all containers in a container group in place. If container image has updates, new image will be downloaded.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Start-AzContainerGroup",
      "description": "Starts all containers in a container group.\nCompute resources will be allocated and billing will start.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}/start",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/start-azcontainergroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Starts all containers in a container group. Compute resources will be allocated and billing will start.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzContainerGroup",
      "description": "Stops all containers in a container group.\nCompute resources will be deallocated and billing will stop.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}/stop",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/stop-azcontainergroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Stops all containers in a container group. Compute resources will be deallocated and billing will stop.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.containerGroupName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
