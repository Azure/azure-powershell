{
  "resourceType": "containerGroups/containers",
  "apiVersion": "2021-09-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.containerinstance"
  },
  "commands": [
    {
      "name": "Add-AzContainerInstanceOutput",
      "description": "Attach to the output stream of a specific container instance in a specified resource group and container group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ContainerInstance/containerGroups/{containerGroupName}/containers/{containerName}/attach",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.containerinstance/add-azcontainerinstanceoutput"
        },
        "parameterSets": [
          {
            "parameters": [
              "-GroupName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Attach to the output stream of a specific container instance in a specified resource group and container group.",
          "parameters": [
            {
              "value": "[Path.containerGroupName]",
              "name": "-GroupName"
            },
            {
              "value": "[Path.containerName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
