{
  "resourceType": "privateDnsZones/virtualNetworkLinks",
  "apiVersion": "2024-06-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.privatedns"
  },
  "commands": [
    {
      "name": "Get-AzPrivateDnsVirtualNetworkLink",
      "description": "Gets a virtual network link to the specified Private DNS zone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/{privateZoneName}/virtualNetworkLinks/{virtualNetworkLinkName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.privatedns/get-azprivatednsvirtualnetworklink"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-PrivateZoneName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a virtual network link to the specified Private DNS zone.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.virtualNetworkLinkName]"
            },
            {
              "name": "-PrivateZoneName",
              "value": "[Path.privateZoneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPrivateDnsVirtualNetworkLink",
      "description": "Deletes a virtual network link to the specified Private DNS zone.\nWARNING: In case of a registration virtual network, all auto-registered DNS records in the zone for the virtual network will also be deleted.\nThis operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/{privateZoneName}/virtualNetworkLinks/{virtualNetworkLinkName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.privatedns/remove-azprivatednsvirtualnetworklink"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-PrivateZoneName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a virtual network link to the specified Private DNS zone. WARNING: In case of a registration virtual network, all auto-registered DNS records in the zone for the virtual network will also be deleted. This operation cannot be undone.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.virtualNetworkLinkName]"
            },
            {
              "name": "-PrivateZoneName",
              "value": "[Path.privateZoneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
