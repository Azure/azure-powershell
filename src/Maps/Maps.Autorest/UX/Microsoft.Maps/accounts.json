{
  "resourceType": "accounts",
  "apiVersion": "2021-02-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.maps"
  },
  "commands": [
    {
      "name": "Get-AzMapsAccount",
      "description": "Get a Maps Account.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Maps/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.maps/get-azmapsaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Maps Account.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzMapsAccount",
      "description": "Delete a Maps Account.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Maps/accounts/{accountName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.maps/remove-azmapsaccount"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Maps Account.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.accountName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
