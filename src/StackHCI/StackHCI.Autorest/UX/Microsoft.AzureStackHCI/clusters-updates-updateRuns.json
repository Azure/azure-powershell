  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.stackhci"
  },
  "commands": [
    {
      "name": "Get-AzStackHciUpdateRun",
      "description": "Get the Update run for a specified update",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AzureStackHCI/clusters/{clusterName}/updates/{updateName}/updateRuns/{updateRunName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.stackhci/get-azstackhciupdaterun"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-UpdateName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the Update run for a specified update",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.updateRunName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-UpdateName",
              "value": "[Path.updateName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzStackHciUpdateRun",
      "description": "Delete specified Update Run",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AzureStackHCI/clusters/{clusterName}/updates/{updateName}/updateRuns/{updateRunName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.stackhci/remove-azstackhciupdaterun"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-UpdateName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete specified Update Run",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.updateRunName]"
            },
            {
              "name": "-ResourceGroupName",
