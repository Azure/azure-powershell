{
  "resourceType": "Spring/apps",
  "apiVersion": "2022-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.springcloud"
  },
  "commands": [
    {
      "name": "Remove-AzSpringCloudApp",
      "description": "Operation to delete an App.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/remove-azspringcloudapp"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Operation to delete an App.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.appName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-ServiceName",
              "value": "[Path.serviceName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
