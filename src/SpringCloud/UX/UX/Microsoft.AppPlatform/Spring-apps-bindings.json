{
  "resourceType": "Spring/apps/bindings",
  "apiVersion": "2022-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.springcloud"
  },
  "commands": [
    {
      "name": "Get-AzSpringCloudAppBinding",
      "description": "Get a Binding and its properties.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}/bindings/{bindingName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/get-azspringcloudappbinding"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AppName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Binding and its properties.",
          "parameters": [
            {
              "value": "[Path.appName]",
              "name": "-AppName"
            },
            {
              "value": "[Path.bindingName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.serviceName]",
              "name": "-ServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSpringCloudAppBinding",
      "description": "Operation to delete a Binding.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}/bindings/{bindingName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/remove-azspringcloudappbinding"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AppName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Operation to delete a Binding.",
          "parameters": [
            {
              "value": "[Path.appName]",
              "name": "-AppName"
            },
            {
              "value": "[Path.bindingName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.serviceName]",
              "name": "-ServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
