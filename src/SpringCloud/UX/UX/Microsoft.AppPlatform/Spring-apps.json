{
  "resourceType": "Spring/apps",
  "apiVersion": "2022-04-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.springcloud"
  },
  "commands": [
    {
      "name": "Get-AzSpringCloudApp",
      "description": "Get an App and its properties.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/get-azspringcloudapp"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an App and its properties.",
          "parameters": [
            {
              "value": "[Path.appName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.serviceName]",
              "name": "-ServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSpringCloudApp",
      "description": "Operation to delete an App.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/remove-azspringcloudapp"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Operation to delete an App.",
          "parameters": [
            {
              "value": "[Path.appName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.serviceName]",
              "name": "-ServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Test-AzSpringCloudAppCustomDomain",
      "description": "Check the resource name is valid as well as not in use.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.AppPlatform/Spring/{serviceName}/apps/{appName}/validateDomain",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.springcloud/test-azspringcloudappcustomdomain"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AppName <String>",
              "-ResourceGroupName <String>",
              "-ServiceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Check the resource name is valid as well as not in use.",
          "parameters": [
            {
              "value": "[Path.appName]",
              "name": "-AppName"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.serviceName]",
              "name": "-ServiceName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
