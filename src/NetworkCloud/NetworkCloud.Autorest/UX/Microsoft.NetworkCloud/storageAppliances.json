{
  "resourceType": "storageAppliances",
  "apiVersion": "2024-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.networkcloud"
  },
  "commands": [
    {
      "name": "Disable-AzNetworkCloudStorageApplianceRemoteVendorManagement",
      "description": "Disable remote vendor management of the provided storage appliance.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/storageAppliances/{storageApplianceName}/disableRemoteVendorManagement",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/disable-aznetworkcloudstorageapplianceremotevendormanagement"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "-StorageApplianceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Disable remote vendor management of the provided storage appliance.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-StorageApplianceName",
              "value": "[Path.storageApplianceName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzNetworkCloudStorageAppliance",
      "description": "Get properties of the provided storage appliance.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/storageAppliances/{storageApplianceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudstorageappliance"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get properties of the provided storage appliance.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.storageApplianceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
