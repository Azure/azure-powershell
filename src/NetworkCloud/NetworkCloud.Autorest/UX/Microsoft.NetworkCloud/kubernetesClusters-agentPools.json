{
  "resourceType": "kubernetesClusters/agentPools",
  "apiVersion": "2024-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.networkcloud"
  },
  "commands": [
    {
      "name": "Get-AzNetworkCloudAgentPool",
      "description": "Get properties of the provided Kubernetes cluster agent pool.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters/{kubernetesClusterName}/agentPools/{agentPoolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudagentpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-KubernetesClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get properties of the provided Kubernetes cluster agent pool.",
          "parameters": [
            {
              "name": "-KubernetesClusterName",
              "value": "[Path.kubernetesClusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.agentPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkCloudAgentPool",
      "description": "Delete the provided Kubernetes cluster agent pool.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/kubernetesClusters/{kubernetesClusterName}/agentPools/{agentPoolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/remove-aznetworkcloudagentpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-KubernetesClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the provided Kubernetes cluster agent pool.",
          "parameters": [
            {
              "name": "-KubernetesClusterName",
              "value": "[Path.kubernetesClusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.agentPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
