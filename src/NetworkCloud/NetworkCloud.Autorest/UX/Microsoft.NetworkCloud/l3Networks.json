{
  "resourceType": "l3Networks",
  "apiVersion": "2024-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.networkcloud"
  },
  "commands": [
    {
      "name": "Get-AzNetworkCloudL3Network",
      "description": "Get properties of the provided layer 3 (L3) network.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/l3Networks/{l3NetworkName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudl3network"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get properties of the provided layer 3 (L3) network.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.l3NetworkName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkCloudL3Network",
      "description": "Delete the provided layer 3 (L3) network.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/l3Networks/{l3NetworkName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/remove-aznetworkcloudl3network"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the provided layer 3 (L3) network.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.l3NetworkName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
