{
  "resourceType": "virtualMachines/consoles",
  "apiVersion": "2023-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.networkcloud"
  },
  "commands": [
    {
      "name": "Get-AzNetworkCloudConsole",
      "description": "Get properties of the provided virtual machine console.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/virtualMachines/{virtualMachineName}/consoles/{consoleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudconsole"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-VirtualMachineName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get properties of the provided virtual machine console.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.consoleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VirtualMachineName",
              "value": "[Path.virtualMachineName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkCloudConsole",
      "description": "Delete the provided virtual machine console.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/virtualMachines/{virtualMachineName}/consoles/{consoleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/remove-aznetworkcloudconsole"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-VirtualMachineName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the provided virtual machine console.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.consoleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-VirtualMachineName",
              "value": "[Path.virtualMachineName]"
            }
          ]
        }
      ]
    }
  ]
}
