        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudbmckeyset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get baseboard management controller key set of the provided cluster.",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
