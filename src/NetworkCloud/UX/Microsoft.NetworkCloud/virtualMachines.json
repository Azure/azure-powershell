      "name": "Get-AzNetworkCloudVirtualMachine",
      "description": "Get properties of the provided virtual machine.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkCloud/virtualMachines/{virtualMachineName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkcloud/get-aznetworkcloudvirtualmachine"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get properties of the provided virtual machine.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.virtualMachineName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
