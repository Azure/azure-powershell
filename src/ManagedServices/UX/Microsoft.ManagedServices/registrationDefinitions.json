{
  "resourceType": "registrationDefinitions",
  "apiVersion": "2020-02-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.managedservices"
  },
  "commands": [
    {
      "name": "Get-AzManagedServicesDefinition",
      "description": "Gets the registration definition details.",
      "path": "/{scope}/providers/Microsoft.ManagedServices/registrationDefinitions/{registrationDefinitionId}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managedservices/get-azmanagedservicesdefinition"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the registration definition details.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.registrationDefinitionId]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzManagedServicesDefinition",
      "description": "Deletes the registration definition.",
      "path": "/{scope}/providers/Microsoft.ManagedServices/registrationDefinitions/{registrationDefinitionId}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managedservices/remove-azmanagedservicesdefinition"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the registration definition.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.registrationDefinitionId]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
    }
  ]
