{
  "resourceType": "migrateProjects/solutions",
  "apiVersion": "2018-09-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.migrate"
  },
  "commands": [
    {
      "name": "Get-AzMigrateSolution",
      "description": "Gets a solution in the migrate project.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Migrate/migrateProjects/{migrateProjectName}/solutions/{solutionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.migrate/get-azmigratesolution"
        },
        "parameterSets": [
          {
            "parameters": [
              "-MigrateProjectName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a solution in the migrate project.",
          "parameters": [
            {
              "name": "-MigrateProjectName",
              "value": "[Path.migrateProjectName]"
            },
            {
              "name": "-Name",
              "value": "[Path.solutionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
