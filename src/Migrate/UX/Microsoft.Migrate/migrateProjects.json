{
  "resourceType": "migrateProjects",
  "apiVersion": "2018-09-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.migrate"
  },
  "commands": [
    {
      "name": "Get-AzMigrateProject",
      "description": "Method to get a migrate project.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Migrate/migrateProjects/{migrateProjectName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.migrate/get-azmigrateproject"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Method to get a migrate project.",
          "parameters": [
            {
              "value": "[Path.migrateProjectName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Register-AzMigrateProjectTool",
      "description": "Registers a tool with the migrate project.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Migrate/migrateProjects/{migrateProjectName}/registerTool",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.migrate/register-azmigrateprojecttool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-MigrateProjectName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Registers a tool with the migrate project.",
          "parameters": [
            {
              "value": "[Path.migrateProjectName]",
              "name": "-MigrateProjectName"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzMigrateProject",
      "description": "Delete the migrate project.\nDeleting non-existent project is a no-operation.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Migrate/migrateProjects/{migrateProjectName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.migrate/remove-azmigrateproject"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete the migrate project. Deleting non-existent project is a no-operation.",
          "parameters": [
            {
              "value": "[Path.migrateProjectName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
