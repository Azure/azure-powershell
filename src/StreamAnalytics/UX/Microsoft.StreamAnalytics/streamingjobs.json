{
  "resourceType": "streamingjobs",
  "apiVersion": "2017-04-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics"
  },
  "commands": [
    {
      "name": "Get-AzStreamAnalyticsJob",
      "description": "Gets details about the specified streaming job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/get-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets details about the specified streaming job.",
          "parameters": [
            {
              "value": "[Path.jobName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzStreamAnalyticsJob",
      "description": "Deletes a streaming job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/remove-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a streaming job.",
          "parameters": [
            {
              "value": "[Path.jobName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Start-AzStreamAnalyticsJob",
      "description": "Starts a streaming job.\nOnce a job is started it will start processing input events and produce output.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}/start",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/start-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Starts a streaming job. Once a job is started it will start processing input events and produce output.",
          "parameters": [
            {
              "value": "[Path.jobName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzStreamAnalyticsJob",
      "description": "Stops a running streaming job.\nThis will cause a running streaming job to stop processing input events and producing output.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}/stop",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/stop-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Stops a running streaming job. This will cause a running streaming job to stop processing input events and producing output.",
          "parameters": [
            {
              "value": "[Path.jobName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
