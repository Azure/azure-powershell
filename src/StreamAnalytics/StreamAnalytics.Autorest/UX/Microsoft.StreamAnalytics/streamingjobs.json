{
  "resourceType": "streamingjobs",
  "apiVersion": "2017-04-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics"
  },
  "commands": [
    {
      "name": "Remove-AzStreamAnalyticsJob",
      "description": "Deletes a streaming job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/remove-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a streaming job.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzStreamAnalyticsJob",
      "description": "Stops a running streaming job.\nThis will cause a running streaming job to stop processing input events and producing output.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StreamAnalytics/streamingjobs/{jobName}/stop",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.streamanalytics/stop-azstreamanalyticsjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Stops a running streaming job. This will cause a running streaming job to stop processing input events and producing output.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
