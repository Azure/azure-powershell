                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccounts_staaccountt01_name'))]"
            ],
            "properties": {
                "accessTier": "TransactionOptimized",
                "shareQuota": 5120,
                "enabledProtocols": "SMB"
            }
        },
        {
            "type": "Microsoft.Storage/storageAccounts/fileServices/shares",
            "apiVersion": "2020-08-01-preview",
            "name": "[concat(parameters('storageAccounts_staaccountt01_name'), '/default/azureml-filestore-87a5cf9f-d08c-4bf6-ae93-9edb23902ff7')]",
