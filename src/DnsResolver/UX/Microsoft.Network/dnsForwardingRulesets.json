{
  "resourceType": "dnsForwardingRulesets",
  "apiVersion": "2022-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver"
  },
  "commands": [
    {
      "name": "Get-AzDnsForwardingRuleset",
      "description": "Gets a DNS forwarding ruleset properties.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/get-azdnsforwardingruleset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a DNS forwarding ruleset properties.",
          "parameters": [
            {
              "value": "[Path.dnsForwardingRulesetName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsForwardingRuleset",
      "description": "Deletes a DNS forwarding ruleset.\nWARNING: This operation cannot be undone.\nAll forwarding rules within the ruleset will be deleted.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/remove-azdnsforwardingruleset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a DNS forwarding ruleset. WARNING: This operation cannot be undone. All forwarding rules within the ruleset will be deleted.",
          "parameters": [
            {
              "value": "[Path.dnsForwardingRulesetName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
