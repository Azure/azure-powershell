{
  "resourceType": "dnsResolvers/outboundEndpoints",
  "apiVersion": "2022-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver"
  },
  "commands": [
    {
      "name": "Get-AzDnsResolverOutboundEndpoint",
      "description": "Gets properties of an outbound endpoint for a DNS resolver.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}/outboundEndpoints/{outboundEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/get-azdnsresolveroutboundendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsResolverName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of an outbound endpoint for a DNS resolver.",
          "parameters": [
            {
              "name": "-DnsResolverName",
              "value": "[Path.dnsResolverName]"
            },
            {
              "name": "-Name",
              "value": "[Path.outboundEndpointName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsResolverOutboundEndpoint",
      "description": "Deletes an outbound endpoint for a DNS resolver.\nWARNING: This operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}/outboundEndpoints/{outboundEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/remove-azdnsresolveroutboundendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsResolverName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an outbound endpoint for a DNS resolver. WARNING: This operation cannot be undone.",
          "parameters": [
            {
              "name": "-DnsResolverName",
              "value": "[Path.dnsResolverName]"
            },
            {
              "name": "-Name",
              "value": "[Path.outboundEndpointName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
