{
  "resourceType": "dnsResolvers/inboundEndpoints",
  "apiVersion": "2022-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver"
  },
  "commands": [
    {
      "name": "Get-AzDnsResolverInboundEndpoint",
      "description": "Gets properties of an inbound endpoint for a DNS resolver.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}/inboundEndpoints/{inboundEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/get-azdnsresolverinboundendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsResolverName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of an inbound endpoint for a DNS resolver.",
          "parameters": [
            {
              "value": "[Path.dnsResolverName]",
              "name": "-DnsResolverName"
            },
            {
              "value": "[Path.inboundEndpointName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsResolverInboundEndpoint",
      "description": "Deletes an inbound endpoint for a DNS resolver.\nWARNING: This operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}/inboundEndpoints/{inboundEndpointName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/remove-azdnsresolverinboundendpoint"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsResolverName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an inbound endpoint for a DNS resolver. WARNING: This operation cannot be undone.",
          "parameters": [
            {
              "value": "[Path.dnsResolverName]",
              "name": "-DnsResolverName"
            },
            {
              "value": "[Path.inboundEndpointName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
