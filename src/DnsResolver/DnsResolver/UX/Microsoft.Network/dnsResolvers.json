{
  "resourceType": "dnsResolvers",
  "apiVersion": "2022-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver"
  },
  "commands": [
    {
      "name": "Get-AzDnsResolver",
      "description": "Gets properties of a DNS resolver.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/get-azdnsresolver"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of a DNS resolver.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dnsResolverName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsResolver",
      "description": "Deletes a DNS resolver.\nWARNING: This operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsResolvers/{dnsResolverName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/remove-azdnsresolver"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a DNS resolver. WARNING: This operation cannot be undone.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.dnsResolverName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
