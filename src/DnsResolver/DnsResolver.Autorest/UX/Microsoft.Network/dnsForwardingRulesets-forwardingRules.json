{
  "resourceType": "dnsForwardingRulesets/forwardingRules",
  "apiVersion": "2023-07-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver"
  },
  "commands": [
    {
      "name": "Get-AzDnsForwardingRulesetForwardingRule",
      "description": "Gets properties of a forwarding rule in a DNS forwarding ruleset.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}/forwardingRules/{forwardingRuleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/get-azdnsforwardingrulesetforwardingrule"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsForwardingRulesetName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of a forwarding rule in a DNS forwarding ruleset.",
          "parameters": [
            {
              "name": "-DnsForwardingRulesetName",
              "value": "[Path.dnsForwardingRulesetName]"
            },
            {
              "name": "-Name",
              "value": "[Path.forwardingRuleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsForwardingRulesetForwardingRule",
      "description": "Deletes a forwarding rule in a DNS forwarding ruleset.\nWARNING: This operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsForwardingRulesets/{dnsForwardingRulesetName}/forwardingRules/{forwardingRuleName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dnsresolver/remove-azdnsforwardingrulesetforwardingrule"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DnsForwardingRulesetName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a forwarding rule in a DNS forwarding ruleset. WARNING: This operation cannot be undone.",
          "parameters": [
            {
              "name": "-DnsForwardingRulesetName",
              "value": "[Path.dnsForwardingRulesetName]"
            },
            {
              "name": "-Name",
              "value": "[Path.forwardingRuleName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
