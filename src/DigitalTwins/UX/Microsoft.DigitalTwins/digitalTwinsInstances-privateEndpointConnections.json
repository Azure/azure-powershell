{
  "resourceType": "digitalTwinsInstances/privateEndpointConnections",
  "apiVersion": "2022-05-31",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.digitaltwins"
  },
  "commands": [
    {
      "name": "Get-AzDigitalTwinsPrivateEndpointConnection",
      "description": "Get private endpoint connection properties for the given private endpoint.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DigitalTwins/digitalTwinsInstances/{resourceName}/privateEndpointConnections/{privateEndpointConnectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.digitaltwins/get-azdigitaltwinsprivateendpointconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ResourceName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get private endpoint connection properties for the given private endpoint.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.privateEndpointConnectionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-ResourceName",
              "value": "[Path.resourceName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDigitalTwinsPrivateEndpointConnection",
      "description": "Delete private endpoint connection with the specified name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DigitalTwins/digitalTwinsInstances/{resourceName}/privateEndpointConnections/{privateEndpointConnectionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.digitaltwins/remove-azdigitaltwinsprivateendpointconnection"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-ResourceName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete private endpoint connection with the specified name.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.privateEndpointConnectionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-ResourceName",
              "value": "[Path.resourceName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
