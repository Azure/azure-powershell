{
  "resourceType": "dnsZones",
  "apiVersion": "2023-07-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.dns"
  },
  "commands": [
    {
      "name": "Get-AzDnsDnssecConfig",
      "description": "Gets the DNSSEC configuration.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/dnssecConfigs/default",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/get-azdnsdnssecconfig"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-ZoneName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the DNSSEC configuration.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-ZoneName",
              "value": "[Path.zoneName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzDnsRecordSet",
      "description": "Gets a record set.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/{recordType}/{relativeRecordSetName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/get-azdnsrecordset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-RecordType <Azure>",
              "-RelativeRecordSetName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-ZoneName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a record set.",
          "parameters": [
            {
              "name": "-RecordType",
              "value": "[Path.recordType]"
            },
            {
              "name": "-RelativeRecordSetName",
              "value": "[Path.relativeRecordSetName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-ZoneName",
              "value": "[Path.zoneName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Get-AzDnsZone",
      "description": "Gets a DNS zone.\nRetrieves the zone properties, but not the record sets within the zone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/get-azdnszone"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets a DNS zone. Retrieves the zone properties, but not the record sets within the zone.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.zoneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsDnssecConfig",
      "description": "Deletes the DNSSEC configuration on a DNS zone.\nThis operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/dnssecConfigs/default",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/remove-azdnsdnssecconfig"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-ZoneName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the DNSSEC configuration on a DNS zone. This operation cannot be undone.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-ZoneName",
              "value": "[Path.zoneName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsRecordSet",
      "description": "Deletes a record set from a DNS zone.\nThis operation cannot be undone.\nRecord sets of type SOA cannot be deleted (they are deleted when the DNS zone is deleted).",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}/{recordType}/{relativeRecordSetName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/remove-azdnsrecordset"
        },
        "parameterSets": [
          {
            "parameters": [
              "-RecordType <Azure>",
              "-RelativeRecordSetName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-ZoneName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a record set from a DNS zone. This operation cannot be undone. Record sets of type SOA cannot be deleted (they are deleted when the DNS zone is deleted).",
          "parameters": [
            {
              "name": "-RecordType",
              "value": "[Path.recordType]"
            },
            {
              "name": "-RelativeRecordSetName",
              "value": "[Path.relativeRecordSetName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-ZoneName",
              "value": "[Path.zoneName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDnsZone",
      "description": "Deletes a DNS zone.\nWARNING: All DNS records in the zone will also be deleted.\nThis operation cannot be undone.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/dnsZones/{zoneName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.dns/remove-azdnszone"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a DNS zone. WARNING: All DNS records in the zone will also be deleted. This operation cannot be undone.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.zoneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
