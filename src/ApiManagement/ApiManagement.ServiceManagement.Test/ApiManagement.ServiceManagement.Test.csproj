  <PropertyGroup>
    <PsModuleName>ApiManagement.ServiceManagement</PsModuleName>
  </PropertyGroup>

  <Import Project="$(MSBuildThisFileDirectory)..\..\Az.Test.props" />

  <PropertyGroup>
