{
  "resourceType": "managedEnvironments/certificates",
  "apiVersion": "2023-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.app"
  },
  "commands": [
    {
      "name": "Get-AzContainerAppManagedEnvCert",
      "description": "Get the specified Certificate.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/managedEnvironments/{environmentName}/certificates/{certificateName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappmanagedenvcert"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the specified Certificate.",
          "parameters": [
            {
              "name": "-EnvName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.certificateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerAppManagedEnvCert",
      "description": "Deletes the specified Certificate.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/managedEnvironments/{environmentName}/certificates/{certificateName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/remove-azcontainerappmanagedenvcert"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the specified Certificate.",
          "parameters": [
            {
              "name": "-EnvName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.certificateName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
