{
  "resourceType": "jobs",
  "apiVersion": "2023-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.app"
  },
  "commands": [
    {
      "name": "Get-AzContainerAppJob",
      "description": "Get the properties of a Container Apps Job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/jobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the properties of a Container Apps Job.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerAppJob",
      "description": "Delete a Container Apps Job.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/jobs/{jobName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/remove-azcontainerappjob"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Container Apps Job.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.jobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
