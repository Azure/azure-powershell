      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappavailableworkloadprofile"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Location <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get all available workload profiles for a location.",
          "parameters": [
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
