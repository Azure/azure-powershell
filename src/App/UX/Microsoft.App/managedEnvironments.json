{
  "resourceType": "managedEnvironments",
  "apiVersion": "2022-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.app"
  },
  "commands": [
    {
      "name": "Get-AzContainerAppManagedEnv",
      "description": "Get the properties of a Managed Environment used to host container apps.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/managedEnvironments/{environmentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappmanagedenv"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get the properties of a Managed Environment used to host container apps.",
          "parameters": [
            {
              "name": "-EnvName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerAppManagedEnv",
      "description": "Delete a Managed Environment if it does not have any container apps.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/managedEnvironments/{environmentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/remove-azcontainerappmanagedenv"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EnvName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Managed Environment if it does not have any container apps.",
          "parameters": [
            {
              "name": "-EnvName",
              "value": "[Path.environmentName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
