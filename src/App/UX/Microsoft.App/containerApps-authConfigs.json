{
  "resourceType": "containerApps/authConfigs",
  "apiVersion": "2022-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.app"
  },
  "commands": [
    {
      "name": "Get-AzContainerAppAuthConfig",
      "description": "Get a AuthConfig of a Container App.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/containerApps/{containerAppName}/authConfigs/{authConfigName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappauthconfig"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AuthConfigName <String>",
              "-ContainerAppName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a AuthConfig of a Container App.",
          "parameters": [
            {
              "name": "-AuthConfigName",
              "value": "[Path.authConfigName]"
            },
            {
              "name": "-ContainerAppName",
              "value": "[Path.containerAppName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerAppAuthConfig",
      "description": "Delete a Container App AuthConfig.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/containerApps/{containerAppName}/authConfigs/{authConfigName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/remove-azcontainerappauthconfig"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AuthConfigName <String>",
              "-ContainerAppName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Container App AuthConfig.",
          "parameters": [
            {
              "name": "-AuthConfigName",
              "value": "[Path.authConfigName]"
            },
            {
              "name": "-ContainerAppName",
              "value": "[Path.containerAppName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
