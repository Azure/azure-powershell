{
  "resourceType": "connectedEnvironments/daprComponents",
  "apiVersion": "2023-05-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.app"
  },
  "commands": [
    {
      "name": "Get-AzContainerAppConnectedEnvDapr",
      "description": "Get a dapr component.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/connectedEnvironments/{connectedEnvironmentName}/daprComponents/{componentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/get-azcontainerappconnectedenvdapr"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ConnectedEnvironmentName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a dapr component.",
          "parameters": [
            {
              "name": "-ConnectedEnvironmentName",
              "value": "[Path.connectedEnvironmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.componentName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzContainerAppConnectedEnvDapr",
      "description": "Delete a Dapr Component from a connected environment.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.App/connectedEnvironments/{connectedEnvironmentName}/daprComponents/{componentName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.app/remove-azcontainerappconnectedenvdapr"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ConnectedEnvironmentName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Dapr Component from a connected environment.",
          "parameters": [
            {
              "name": "-ConnectedEnvironmentName",
              "value": "[Path.connectedEnvironmentName]"
            },
            {
              "name": "-Name",
              "value": "[Path.componentName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
