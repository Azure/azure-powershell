  "commands": [
    {
      "name": "Get-AzSapMonitorProviderInstance",
      "description": "Gets properties of a provider instance for the specified subscription, resource group, SapMonitor name, and resource name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HanaOnAzure/sapMonitors/{sapMonitorName}/providerInstances/{providerInstanceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.hanaonazure/get-azsapmonitorproviderinstance"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-SapMonitorName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of a provider instance for the specified subscription, resource group, SapMonitor name, and resource name.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.providerInstanceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SapMonitorName",
              "value": "[Path.sapMonitorName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzSapMonitorProviderInstance",
      "description": "Deletes a provider instance for the specified subscription, resource group, SapMonitor name, and resource name.",
