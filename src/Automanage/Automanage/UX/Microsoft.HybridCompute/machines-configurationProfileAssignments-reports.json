{
  "resourceType": "machines/configurationProfileAssignments/reports",
  "apiVersion": "2022-05-04",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.automanage"
  },
  "commands": [
    {
      "name": "Get-AzAutomanageHcrpReport",
      "description": "Get information about a report associated with a configuration profile assignment run",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HybridCompute/machines/{machineName}/providers/Microsoft.Automanage/configurationProfileAssignments/{configurationProfileAssignmentName}/reports/{reportName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.automanage/get-azautomanagehcrpreport"
        },
        "parameterSets": [
          {
            "parameters": [
              "-MachineName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get information about a report associated with a configuration profile assignment run",
          "parameters": [
            {
              "name": "-MachineName",
              "value": "[Path.machineName]"
            },
            {
              "name": "-Name",
              "value": "[Path.reportName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
