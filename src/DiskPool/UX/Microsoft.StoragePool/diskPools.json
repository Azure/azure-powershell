{
  "resourceType": "diskPools",
  "apiVersion": "2021-08-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.diskpool"
  },
  "commands": [
    {
      "name": "Get-AzDiskPool",
      "description": "Get a Disk pool.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StoragePool/diskPools/{diskPoolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/get-azdiskpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get a Disk pool.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Invoke-AzDiskPoolRedeployment",
      "description": "Upgrade replaces the underlying virtual machine hosts one at a time.\nThis operation can take 10-15 minutes to complete.\nThis is expected service behavior.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StoragePool/diskPools/{diskPoolName}/upgrade",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/invoke-azdiskpoolredeployment"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DiskPoolName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Upgrade replaces the underlying virtual machine hosts one at a time. This operation can take 10-15 minutes to complete. This is expected service behavior.",
          "parameters": [
            {
              "name": "-DiskPoolName",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzDiskPool",
      "description": "Delete a Disk pool; attached disks are not affected.\nThis delete operation can take 10 minutes to complete.\nThis is expected service behavior.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StoragePool/diskPools/{diskPoolName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/remove-azdiskpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete a Disk pool; attached disks are not affected. This delete operation can take 10 minutes to complete. This is expected service behavior.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Start-AzDiskPool",
      "description": "The operation to start a Disk Pool.\nThis start operation can take 10 minutes to complete.\nThis is expected service behavior.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StoragePool/diskPools/{diskPoolName}/start",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/start-azdiskpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "The operation to start a Disk Pool. This start operation can take 10 minutes to complete. This is expected service behavior.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzDiskPool",
      "description": "Shuts down the Disk Pool and releases the compute resources.\nYou are not billed for the compute resources that this Disk Pool uses.\nThis operation can take 10 minutes to complete.\nThis is expected service behavior.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StoragePool/diskPools/{diskPoolName}/deallocate",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/stop-azdiskpool"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Shuts down the Disk Pool and releases the compute resources. You are not billed for the compute resources that this Disk Pool uses. This operation can take 10 minutes to complete. This is expected service behavior.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
