      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.diskpool/remove-azdiskpooliscsitarget"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DiskPoolName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete an iSCSI Target.",
          "parameters": [
            {
              "name": "-DiskPoolName",
              "value": "[Path.diskPoolName]"
            },
            {
              "name": "-Name",
              "value": "[Path.iscsiTargetName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
