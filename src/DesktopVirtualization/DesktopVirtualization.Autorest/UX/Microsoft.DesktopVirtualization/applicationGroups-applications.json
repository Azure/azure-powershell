  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.desktopvirtualization"
  },
  "commands": [
    {
      "name": "Get-AzWvdApplication",
      "description": "Get an application.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DesktopVirtualization/applicationGroups/{applicationGroupName}/applications/{applicationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.desktopvirtualization/get-azwvdapplication"
        },
        "parameterSets": [
          {
            "parameters": [
              "-GroupName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an application.",
          "parameters": [
            {
              "name": "-GroupName",
              "value": "[Path.applicationGroupName]"
            },
            {
              "name": "-Name",
              "value": "[Path.applicationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzWvdApplication",
      "description": "Remove an application.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DesktopVirtualization/applicationGroups/{applicationGroupName}/applications/{applicationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.desktopvirtualization/remove-azwvdapplication"
        },
        "parameterSets": [
          {
            "parameters": [
              "-GroupName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Remove an application.",
          "parameters": [
            {
              "name": "-GroupName",
              "value": "[Path.applicationGroupName]"
            },
            {
              "name": "-Name",
              "value": "[Path.applicationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
