{
  "resourceType": "workspaces",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis"
  },
  "commands": [
    {
      "name": "Get-AzHealthcareApisWorkspace",
      "description": "Gets the properties of the specified workspace.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/get-azhealthcareapisworkspace"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the properties of the specified workspace.",
          "parameters": [
            {
              "value": "[Path.workspaceName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzHealthcareApisWorkspace",
      "description": "Deletes a specified workspace.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/remove-azhealthcareapisworkspace"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a specified workspace.",
          "parameters": [
            {
              "value": "[Path.workspaceName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
