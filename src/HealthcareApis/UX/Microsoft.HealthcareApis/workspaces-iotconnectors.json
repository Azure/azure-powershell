{
  "resourceType": "workspaces/iotconnectors",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis"
  },
  "commands": [
    {
      "name": "Get-AzHealthcareIotConnector",
      "description": "Gets the properties of the specified IoT Connector.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}/iotconnectors/{iotConnectorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/get-azhealthcareiotconnector"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the properties of the specified IoT Connector.",
          "parameters": [
            {
              "value": "[Path.iotConnectorName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            },
            {
              "value": "[Path.workspaceName]",
              "name": "-WorkspaceName"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzHealthcareIotConnector",
      "description": "Deletes an IoT Connector.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}/iotconnectors/{iotConnectorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/remove-azhealthcareiotconnector"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an IoT Connector.",
          "parameters": [
            {
              "value": "[Path.iotConnectorName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            },
            {
              "value": "[Path.workspaceName]",
              "name": "-WorkspaceName"
            }
          ]
        }
      ]
    }
  ]
}
