{
  "resourceType": "workspaces/fhirservices",
  "apiVersion": "2021-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis"
  },
  "commands": [
    {
      "name": "Get-AzHealthcareFhirService",
      "description": "Gets the properties of the specified FHIR Service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}/fhirservices/{fhirServiceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/get-azhealthcarefhirservice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the properties of the specified FHIR Service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.fhirServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzHealthcareFhirService",
      "description": "Deletes a FHIR Service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.HealthcareApis/workspaces/{workspaceName}/fhirservices/{fhirServiceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.healthcareapis/remove-azhealthcarefhirservice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]",
              "-WorkspaceName <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a FHIR Service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.fhirServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            },
            {
              "name": "-WorkspaceName",
              "value": "[Path.workspaceName]"
            }
          ]
        }
      ]
    }
  ]
}
