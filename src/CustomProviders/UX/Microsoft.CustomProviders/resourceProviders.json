{
  "resourceType": "resourceProviders",
  "apiVersion": "2018-09-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.customproviders"
  },
  "commands": [
    {
      "name": "Get-AzCustomProvider",
      "description": "Gets the custom resource provider manifest.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.CustomProviders/resourceProviders/{resourceProviderName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.customproviders/get-azcustomprovider"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the custom resource provider manifest.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.resourceProviderName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzCustomProvider",
      "description": "Deletes the custom resource provider.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.CustomProviders/resourceProviders/{resourceProviderName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.customproviders/remove-azcustomprovider"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the custom resource provider.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.resourceProviderName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
