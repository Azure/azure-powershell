{
  "resourceType": "associations",
  "apiVersion": "2018-09-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.customproviders"
  },
  "commands": [
    {
      "name": "Get-AzCustomProviderAssociation",
      "description": "Get an association.",
      "path": "/{scope}/providers/Microsoft.CustomProviders/associations/{associationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.customproviders/get-azcustomproviderassociation"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get an association.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.associationName]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzCustomProviderAssociation",
      "description": "Delete an association.",
      "path": "/{scope}/providers/Microsoft.CustomProviders/associations/{associationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.customproviders/remove-azcustomproviderassociation"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-Scope <String>"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Delete an association.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.associationName]"
            },
            {
              "name": "-Scope",
              "value": "[Path.scope]"
            }
          ]
        }
      ]
    }
  ]
}
