{
  "resourceType": "azureTrafficCollectors",
  "apiVersion": "2022-08-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.networkfunction"
  },
  "commands": [
    {
      "name": "Get-AzNetworkFunctionTrafficCollector",
      "description": "Gets the specified Azure Traffic Collector in a specified resource group",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkFunction/azureTrafficCollectors/{azureTrafficCollectorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkfunction/get-aznetworkfunctiontrafficcollector"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the specified Azure Traffic Collector in a specified resource group",
          "parameters": [
            {
              "value": "[Path.azureTrafficCollectorName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkFunctionTrafficCollector",
      "description": "Deletes a specified Azure Traffic Collector resource.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.NetworkFunction/azureTrafficCollectors/{azureTrafficCollectorName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.networkfunction/remove-aznetworkfunctiontrafficcollector"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a specified Azure Traffic Collector resource.",
          "parameters": [
            {
              "value": "[Path.azureTrafficCollectorName]",
              "name": "-Name"
            },
            {
              "value": "[Path.resourceGroupName]",
              "name": "-ResourceGroupName"
            },
            {
              "value": "[Path.subscriptionId]",
              "name": "-SubscriptionId"
            }
          ]
        }
      ]
    }
  ]
}
