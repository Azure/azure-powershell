{
  "resourceType": "packetCoreControlPlanes",
  "apiVersion": "2022-11-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.mobilenetwork"
  },
  "commands": [
    {
      "name": "Deploy-AzMobileNetworkReinstallPacketCoreControlPlane",
      "description": "Reinstall the specified packet core control plane.\nThis action will remove any transaction state from the packet core to return it to a known state.\nThis action will cause a service outage.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/packetCoreControlPlanes/{packetCoreControlPlaneName}/reinstall",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.mobilenetwork/deploy-azmobilenetworkreinstallpacketcorecontrolplane"
        },
        "parameterSets": [
          {
            "parameters": [
              "-PacketCoreControlPlaneName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Reinstall the specified packet core control plane. This action will remove any transaction state from the packet core to return it to a known state. This action will cause a service outage.",
          "parameters": [
            {
              "name": "-PacketCoreControlPlaneName",
              "value": "[Path.packetCoreControlPlaneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Deploy-AzMobileNetworkRollbackPacketCoreControlPlane",
      "description": "Roll back the specified packet core control plane to the previous version, \\\"rollbackVersion\\\".\nMultiple consecutive rollbacks are not possible.\nThis action may cause a service outage.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.MobileNetwork/packetCoreControlPlanes/{packetCoreControlPlaneName}/rollback",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.mobilenetwork/deploy-azmobilenetworkrollbackpacketcorecontrolplane"
        },
        "parameterSets": [
          {
            "parameters": [
              "-PacketCoreControlPlaneName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Roll back the specified packet core control plane to the previous version, \\\"rollbackVersion\\\". Multiple consecutive rollbacks are not possible. This action may cause a service outage.",
          "parameters": [
            {
              "name": "-PacketCoreControlPlaneName",
              "value": "[Path.packetCoreControlPlaneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
