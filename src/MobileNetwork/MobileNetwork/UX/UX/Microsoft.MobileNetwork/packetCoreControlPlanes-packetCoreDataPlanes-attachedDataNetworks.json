              "value": "[Path.packetCoreControlPlaneName]"
            },
            {
              "name": "-PacketCoreDataPlaneName",
              "value": "[Path.packetCoreDataPlaneName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
