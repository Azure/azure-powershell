{
  "resourceType": "Clusters/sandboxCustomImages",
  "apiVersion": "2023-08-15",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.kusto"
  },
  "commands": [
    {
      "name": "Get-AzKustoSandboxCustomImage",
      "description": "Returns a sandbox custom image",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Kusto/Clusters/{clusterName}/sandboxCustomImages/{sandboxCustomImageName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.kusto/get-azkustosandboxcustomimage"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns a sandbox custom image",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.sandboxCustomImageName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzKustoSandboxCustomImage",
      "description": "Deletes a sandbox custom image.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Kusto/Clusters/{clusterName}/sandboxCustomImages/{sandboxCustomImageName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.kusto/remove-azkustosandboxcustomimage"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a sandbox custom image.",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.sandboxCustomImageName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
