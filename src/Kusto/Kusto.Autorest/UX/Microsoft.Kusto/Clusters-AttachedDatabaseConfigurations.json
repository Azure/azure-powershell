{
  "resourceType": "Clusters/AttachedDatabaseConfigurations",
  "apiVersion": "2024-04-13",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.kusto"
  },
  "commands": [
    {
      "name": "Get-AzKustoAttachedDatabaseConfiguration",
      "description": "Returns an attached database configuration.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Kusto/Clusters/{clusterName}/AttachedDatabaseConfigurations/{attachedDatabaseConfigurationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.kusto/get-azkustoattacheddatabaseconfiguration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns an attached database configuration.",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.attachedDatabaseConfigurationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzKustoAttachedDatabaseConfiguration",
      "description": "Deletes the attached database configuration with the given name.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Kusto/Clusters/{clusterName}/AttachedDatabaseConfigurations/{attachedDatabaseConfigurationName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.kusto/remove-azkustoattacheddatabaseconfiguration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ClusterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the attached database configuration with the given name.",
          "parameters": [
            {
              "name": "-ClusterName",
              "value": "[Path.clusterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.attachedDatabaseConfigurationName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
