{
	"resourceType": "applicationGateways",
	"apiVersion": "2022-05-01",
	"learnMore": {
		"url": "https://learn.microsoft.com/powershell/module/az.network/#network"
	},
	"commands": [
		{
			"name": "Get-AzApplicationGateway",
			"description": "Gets an application gateway.",
			"path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/applicationGateways/{name}",
			"confirmation": false,
			"help": {
				"learnMore": {
					"url": "https://learn.microsoft.com/powershell/module/az.network/get-azapplicationgateway"
				},
				"parameterSets": [
					{
						"parameters": [
							"[-Name <String>]",
							"[-ResourceGroupName <String>]"
						]
					}
				]
			},
			"examples": [
				{
					"description": "Gets an application gateway.",
					"parameters": [
						{
							"name": "-ResourceGroupName",
							"value": "[path.resourceGroupName]"
						},
						{
							"name": "-Name",
							"value": "[path.name]"
						}
					]
				}
			]
		},
		{
			"name": "Remove-AzApplicationGateway",
			"description": "Removes an application gateway.",
			"path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/applicationGateways/{name}",
			"confirmation": false,
			"help": {
				"learnMore": {
					"url": "https://learn.microsoft.com/powershell/module/az.network/remove-azapplicationgateway"
				},
				"parameterSets": [
					{
						"parameters": [
							"[-Name <String>]",
							"[-ResourceGroupName <String>]"
						]
					}
				]
			},
			"examples": [
				{
					"description": "Removes an application gateway.",
					"parameters": [
						{
							"name": "-ResourceGroupName",
							"value": "[path.resourceGroupName]"
						},
						{
							"name": "-Name",
							"value": "[path.name]"
						}
					]
				}
			]
		}
	]
}
