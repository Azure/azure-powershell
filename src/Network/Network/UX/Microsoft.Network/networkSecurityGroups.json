			"confirmation": false,
			"help": {
				"learnMore": {
					"url": "https://learn.microsoft.com/powershell/module/az.network/get-aznetworksecuritygroup"
				},
				"parameterSets": [
					{
						"parameters": [
							"[-Name <String>]",
							"[-ResourceGroupName <String>]"
						]
					}
				]
			},
			"examples": [
				{
					"description": "Gets a network security group.",
					"parameters": [
						{
							"name": "-ResourceGroupName",
							"value": "[path.resourceGroupName]"
						},
						{
							"name": "-Name",
							"value": "[path.name]"
						}
					]
				}
			]
		},
		{
			"name": "Remove-AzNetworkSecurityGroup",
