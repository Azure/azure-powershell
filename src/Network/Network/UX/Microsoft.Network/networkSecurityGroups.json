					{
						"parameters": [
							"[-Name <String>]",
							"[-ResourceGroupName <String>]"
						]
					}
				]
			},
			"examples": [
				{
					"description": "Removes a network security group.",
					"parameters": [
						{
							"name": "-ResourceGroupName",
							"value": "[path.resourceGroupName]"
