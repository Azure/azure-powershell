{
  "resourceType": "virtualMachineScaleSets/virtualMachines",
  "apiVersion": "2022-03-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.compute/#vm-scale-sets"
  },
  "commands": [
    {
      "name": "Get-AzVmssVM",
      "description": "Gets the properties of a VMSS virtual machine.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/virtualMachineScaleSets/{vmScaleSetName}/virtualMachines/{instanceId}",
      "confirmation": false,
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.compute/get-azvmssvm"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ResourceGroupName <String>",
              "-VMScaleSetName <String>",
              "-InstanceId <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the properties of a VMSS virtual machine.",
          "parameters": [
            {
              "name": "-ResourceGroupName",
              "value": "[path.resourceGroupName]"
            },
            {
              "name": "-VMScaleSetName",
              "value": "[path.vmScaleSetName]"
            },
            {
              "name": "-InstanceId",
              "value": "[path.instanceId]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
