  },
  "commands": [
    {
      "name": "Get-AzWebAppSlotTriggeredWebJobHistory",
      "description": "Get or list triggered web job's history for a deployment slot.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Web/sites/{name}/slots/{slot}/triggeredwebjobs/{webJobName}/history/{id}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.websites/get-azwebappslottriggeredwebjobhistory"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AppName <String>",
              "-Id <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "-SlotName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Get or list triggered web job's history for a deployment slot.",
          "parameters": [
            {
              "name": "-AppName",
              "value": "[Path.name]"
            },
            {
              "name": "-Id",
              "value": "[Path.id]"
            },
            {
              "name": "-Name",
              "value": "[Path.webJobName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SlotName",
              "value": "[Path.slot]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
