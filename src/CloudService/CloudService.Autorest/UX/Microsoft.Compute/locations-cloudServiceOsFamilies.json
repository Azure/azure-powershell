{
  "resourceType": "locations/cloudServiceOsFamilies",
  "apiVersion": "2022-09-04",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.cloudservice"
  },
  "commands": [
    {
      "name": "Get-AzCloudServiceOSFamily",
      "description": "Gets properties of a guest operating system family that can be specified in the XML service configuration (.cscfg) for a cloud service.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Compute/locations/{location}/cloudServiceOsFamilies/{osFamilyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cloudservice/get-azcloudserviceosfamily"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Location <String>",
              "-OSFamilyName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of a guest operating system family that can be specified in the XML service configuration (.cscfg) for a cloud service.",
          "parameters": [
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-OSFamilyName",
              "value": "[Path.osFamilyName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
