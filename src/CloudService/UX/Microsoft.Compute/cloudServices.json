      "name": "Get-AzCloudServiceInstanceView",
      "description": "Gets the status of a cloud service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Compute/cloudServices/{cloudServiceName}/instanceView",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cloudservice/get-azcloudserviceinstanceview"
        },
        "parameterSets": [
          {
            "parameters": [
              "-CloudServiceName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the status of a cloud service.",
          "parameters": [
            {
              "name": "-CloudServiceName",
              "value": "[Path.cloudServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
