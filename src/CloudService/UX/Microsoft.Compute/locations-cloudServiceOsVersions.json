{
  "resourceType": "locations/cloudServiceOsVersions",
  "apiVersion": "2022-09-04",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.cloudservice"
  },
  "commands": [
    {
      "name": "Get-AzCloudServiceOSVersion",
      "description": "Gets properties of a guest operating system version that can be specified in the XML service configuration (.cscfg) for a cloud service.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Compute/locations/{location}/cloudServiceOsVersions/{osVersionName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cloudservice/get-azcloudserviceosversion"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Location <String>",
              "-OSVersionName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets properties of a guest operating system version that can be specified in the XML service configuration (.cscfg) for a cloud service.",
          "parameters": [
            {
              "name": "-Location",
              "value": "[Path.location]"
            },
            {
              "name": "-OSVersionName",
              "value": "[Path.osVersionName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
