<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <ViewDefinitions>
    <View>
      <Name>Microsoft.Azure.Commands.Blueprint.Models.PSPublishedBlueprint</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Blueprint.Models.PSPublishedBlueprint</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>BlueprintName</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Id</Label>
                <PropertyName>Id</PropertyName>
              </ListItem>
              <ListItem>
                <Label>DefinitionLocationId</Label>
                <PropertyName>DefinitionLocationId</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Version</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    $_.Description -ne $null -and $_.Description -ne &quot;&quot;
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <PropertyName>ChangeNotes</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    $_.ChangeNotes -ne $null -and $_.ChangeNotes -ne &quot;&quot;
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>TimeCreated</Label>
                <ScriptBlock>$_.Status.TimeCreated.ToString("yyyy-MM-dd")</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>TargetScope</Label>
                <PropertyName>TargetScope</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Parameters</Label>
                <ScriptBlock>$_.Parameters.Keys</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ResourceGroups</Label>
                <ScriptBlock> $_.ResourceGroups.Keys </ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
