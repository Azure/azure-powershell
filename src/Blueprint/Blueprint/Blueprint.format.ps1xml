<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <ViewDefinitions>
    <View>
      <Name>Microsoft.Azure.Commands.Blueprint.Models.PSPublishedBlueprint</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Blueprint.Models.PSPublishedBlueprint</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Id</Label>
                <PropertyName>Id</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>SubscriptionId</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.SubscriptionId))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <PropertyName>ManagementGroupId</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.ManagementGroupId))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>Version</Label>
                <PropertyName>Version</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.Description))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <PropertyName>ChangeNotes</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.ChangeNotes))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>TimeCreated</Label>
                <ScriptBlock>$_.Status.TimeCreated.ToString()</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>TargetScope</Label>
                <PropertyName>TargetScope</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Parameters</Label>
                <ScriptBlock>$_.Parameters.Keys</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ResourceGroups</Label>
                <ScriptBlock> $_.ResourceGroups.Keys </ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
    <View>
      <Name>Microsoft.Azure.Commands.Blueprint.Models.PSBlueprint</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Blueprint.Models.PSBlueprint</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Id</Label>
                <PropertyName>Id</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>SubscriptionId</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.SubscriptionId))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <PropertyName>ManagementGroupId</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.ManagementGroupId))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>Versions</Label>
                <PropertyName>Versions</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    $_.Versions.count -gt 0
                  </ScriptBlock>
                </ItemSelectionCondition>
               </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.Description))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>TimeCreated</Label>
                <ScriptBlock>$_.Status.TimeCreated.ToString()</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>TargetScope</Label>
                <PropertyName>TargetScope</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Parameters</Label>
                <ScriptBlock>$_.Parameters.Keys</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ResourceGroups</Label>
                <ScriptBlock> $_.ResourceGroups.Keys </ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
    <View>
      <Name>Microsoft.Azure.Commands.Blueprint.Models.PSBlueprintAssignment</Name>
      <ViewSelectedBy>
        <TypeName>Microsoft.Azure.Commands.Blueprint.Models.PSBlueprintAssignment</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <Label>Name</Label>
                <PropertyName>Name</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Id</Label>
                <PropertyName>Id</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Scope</Label>
                <PropertyName>Scope</PropertyName>
              </ListItem>
              <ListItem>
                <PropertyName>Description</PropertyName>
                <ItemSelectionCondition>
                  <ScriptBlock>
                    -not ([string]::IsNullOrEmpty($_.Description))
                  </ScriptBlock>
                </ItemSelectionCondition>
              </ListItem>
              <ListItem>
                <Label>LastModified</Label>
                <ScriptBlock>$_.Status.LastModified.ToString("yyyy-MM-dd")</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>LockMode</Label>
                  <ScriptBlock>$_.Locks.Mode.ToString()</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ExcludedActions</Label>
                <ScriptBlock>
                  If (-not ([string]::IsNullOrEmpty($_.Locks.ExcludedActions))){
                    $_.Locks.ExcludedActions
                  }
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ExcludedPrincipals</Label>
                <ScriptBlock>
                  If (-not ([string]::IsNullOrEmpty($_.Locks.ExcludedPrincipals))){
                    $_.Locks.ExcludedPrincipals
                  }
                </ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ProvisioningState</Label>
                <PropertyName>ProvisioningState</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Parameters</Label>
                <ScriptBlock>$_.Parameters.Keys</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>ResourceGroups</Label>
                <ScriptBlock> $_.ResourceGroups.Keys </ScriptBlock>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>
  </ViewDefinitions>
</Configuration>