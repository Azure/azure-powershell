             "parameterSets":[
                {
                   "parameters":[
                     "[[-ResourceGroupName] <System.String>]",
                     "[[-ServerName] <String>]",
                     "[[-DatabaseName] <String>]"
                   ]
                }
             ]
          },
          "examples":[
             {
                "description":"Removes an Azure SQL database.",
                "parameters":[
                   {
