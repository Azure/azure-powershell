{
  "resourceType": "peerings",
  "apiVersion": "2022-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.peering"
  },
  "commands": [
    {
      "name": "Get-AzPeering",
      "description": "Gets an existing peering with the specified name under the given subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peerings/{peeringName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/get-azpeering"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing peering with the specified name under the given subscription and resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peeringName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPeering",
      "description": "Deletes an existing peering with the specified name under the given subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peerings/{peeringName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/remove-azpeering"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing peering with the specified name under the given subscription and resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peeringName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
