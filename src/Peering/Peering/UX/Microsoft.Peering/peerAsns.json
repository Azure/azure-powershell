{
  "resourceType": "peerAsns",
  "apiVersion": "2022-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.peering"
  },
  "commands": [
    {
      "name": "Get-AzPeeringAsn",
      "description": "Gets the peer ASN with the specified name under the given subscription.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Peering/peerAsns/{peerAsnName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/get-azpeeringasn"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets the peer ASN with the specified name under the given subscription.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peerAsnName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPeeringAsn",
      "description": "Deletes an existing peer ASN with the specified name under the given subscription.",
      "path": "/subscriptions/{subscriptionId}/providers/Microsoft.Peering/peerAsns/{peerAsnName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/remove-azpeeringasn"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing peer ASN with the specified name under the given subscription.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peerAsnName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
