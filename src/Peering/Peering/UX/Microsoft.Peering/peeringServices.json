{
  "resourceType": "peeringServices",
  "apiVersion": "2022-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.peering"
  },
  "commands": [
    {
      "name": "Get-AzPeeringService",
      "description": "Gets an existing peering service with the specified name under the given subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peeringServices/{peeringServiceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/get-azpeeringservice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing peering service with the specified name under the given subscription and resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peeringServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPeeringService",
      "description": "Deletes an existing peering service with the specified name under the given subscription and resource group.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peeringServices/{peeringServiceName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/remove-azpeeringservice"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing peering service with the specified name under the given subscription and resource group.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.peeringServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
