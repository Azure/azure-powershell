{
  "resourceType": "peeringServices/connectionMonitorTests",
  "apiVersion": "2022-10-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.peering"
  },
  "commands": [
    {
      "name": "Get-AzPeeringConnectionMonitorTest",
      "description": "Gets an existing connection monitor test with the specified name under the given subscription, resource group and peering service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peeringServices/{peeringServiceName}/connectionMonitorTests/{connectionMonitorTestName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/get-azpeeringconnectionmonitortest"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-PeeringServiceName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing connection monitor test with the specified name under the given subscription, resource group and peering service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.connectionMonitorTestName]"
            },
            {
              "name": "-PeeringServiceName",
              "value": "[Path.peeringServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzPeeringConnectionMonitorTest",
      "description": "Deletes an existing connection monitor test with the specified name under the given subscription, resource group and peering service.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Peering/peeringServices/{peeringServiceName}/connectionMonitorTests/{connectionMonitorTestName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.peering/remove-azpeeringconnectionmonitortest"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-PeeringServiceName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing connection monitor test with the specified name under the given subscription, resource group and peering service.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.connectionMonitorTestName]"
            },
            {
              "name": "-PeeringServiceName",
              "value": "[Path.peeringServiceName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
