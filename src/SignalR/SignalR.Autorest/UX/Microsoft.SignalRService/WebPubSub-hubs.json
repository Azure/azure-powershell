          "description": "Delete a hub setting.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.hubName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-ResourceName",
              "value": "[Path.resourceName]"
            },
            {
