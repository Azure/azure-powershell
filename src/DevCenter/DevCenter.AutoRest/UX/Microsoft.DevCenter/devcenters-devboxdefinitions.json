      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.devcenter/remove-azdevcenteradmindevboxdefinition"
        },
        "parameterSets": [
          {
            "parameters": [
              "-DevCenterName <String>",
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes a Dev Box definition",
          "parameters": [
            {
              "name": "-DevCenterName",
              "value": "[Path.devCenterName]"
            },
            {
              "name": "-Name",
              "value": "[Path.devBoxDefinitionName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
