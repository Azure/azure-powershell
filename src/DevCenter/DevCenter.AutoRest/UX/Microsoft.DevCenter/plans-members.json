          "description": "Deletes a devcenter plan member",
          "parameters": [
            {
              "name": "-MemberName",
              "value": "[Path.memberName]"
            },
            {
              "name": "-PlanName",
              "value": "[Path.planName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
