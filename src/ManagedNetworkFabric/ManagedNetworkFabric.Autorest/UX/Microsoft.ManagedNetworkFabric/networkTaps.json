{
  "resourceType": "networkTaps",
  "apiVersion": "2023-06-15",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric"
  },
  "commands": [
    {
      "name": "Get-AzNetworkFabricTap",
      "description": "Retrieves details of this Network Tap.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkTaps/{networkTapName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric/get-aznetworkfabrictap"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Retrieves details of this Network Tap.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.networkTapName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkFabricTap",
      "description": "Deletes Network Tap.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkTaps/{networkTapName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric/remove-aznetworkfabrictap"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes Network Tap.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.networkTapName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
