{
  "resourceType": "networkFabricControllers",
  "apiVersion": "2023-06-15",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric"
  },
  "commands": [
    {
      "name": "Get-AzNetworkFabricController",
      "description": "Shows the provisioning status of Network Fabric Controller.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkFabricControllers/{networkFabricControllerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric/get-aznetworkfabriccontroller"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Shows the provisioning status of Network Fabric Controller.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.networkFabricControllerName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzNetworkFabricController",
      "description": "Deletes the Network Fabric Controller resource.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.ManagedNetworkFabric/networkFabricControllers/{networkFabricControllerName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.managednetworkfabric/remove-aznetworkfabriccontroller"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes the Network Fabric Controller resource.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.networkFabricControllerName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
