  },
  "commands": [
    {
      "name": "Get-AzCdnOriginGroup",
      "description": "Gets an existing origin group within an endpoint.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/endpoints/{endpointName}/originGroups/{originGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/get-azcdnorigingroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EndpointName <String>",
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing origin group within an endpoint.",
          "parameters": [
            {
              "name": "-EndpointName",
              "value": "[Path.endpointName]"
            },
            {
              "name": "-Name",
              "value": "[Path.originGroupName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzCdnOriginGroup",
      "description": "Deletes an existing origin group within an endpoint.",
