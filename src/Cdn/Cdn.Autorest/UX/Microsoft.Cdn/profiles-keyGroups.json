  },
  "commands": [
    {
      "name": "Get-AzCdnKeyGroup",
      "description": "Gets an existing KeyGroup within a profile.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/keyGroups/{keyGroupName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/get-azcdnkeygroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing KeyGroup within a profile.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.keyGroupName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzCdnKeyGroup",
      "description": "Deletes an existing KeyGroup within profile.",
