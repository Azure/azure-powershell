      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/remove-azcdnkeygroup"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing KeyGroup within profile.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.keyGroupName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
