{
  "resourceType": "profiles",
  "apiVersion": "2024-05-01-preview",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.cdn"
  },
  "commands": [
    {
      "name": "Invoke-AzCdnAbortProfileMigration",
      "description": "Abort the migration to Azure Frontdoor Premium/Standard.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/migrationAbort",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/invoke-azcdnabortprofilemigration"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Abort the migration to Azure Frontdoor Premium/Standard.",
          "parameters": [
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Invoke-AzCdnCanCdnProfile",
      "description": "Checks if CDN profile can be migrated to Azure Frontdoor(Standard/Premium) profile.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/cdnCanMigrateToAfd",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/invoke-azcdncancdnprofile"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Checks if CDN profile can be migrated to Azure Frontdoor(Standard/Premium) profile.",
          "parameters": [
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Move-AzFrontDoorCdnCdnProfileToAFD",
      "description": "Migrate the CDN profile to Azure Frontdoor(Standard/Premium) profile.\nThis step prepares the profile for migration and will be followed by Commit to finalize the migration.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/cdnMigrateToAfd",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/move-azfrontdoorcdncdnprofiletoafd"
        },
        "parameterSets": [
          {
            "parameters": [
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Migrate the CDN profile to Azure Frontdoor(Standard/Premium) profile. This step prepares the profile for migration and will be followed by Commit to finalize the migration.",
          "parameters": [
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
