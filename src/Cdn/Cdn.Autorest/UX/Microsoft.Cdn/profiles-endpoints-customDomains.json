        },
        "parameterSets": [
          {
            "parameters": [
              "-CustomDomainName <String>",
              "-EndpointName <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Disable https delivery of the custom domain.",
          "parameters": [
            {
              "name": "-CustomDomainName",
              "value": "[Path.customDomainName]"
            },
            {
              "name": "-EndpointName",
              "value": "[Path.endpointName]"
            },
