{
  "resourceType": "profiles/securityPolicies",
  "apiVersion": "2024-02-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.cdn"
  },
  "commands": [
    {
      "name": "Get-AzFrontDoorCdnSecurityPolicy",
      "description": "Gets an existing security policy within a profile.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/securityPolicies/{securityPolicyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/get-azfrontdoorcdnsecuritypolicy"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Gets an existing security policy within a profile.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.securityPolicyName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzFrontDoorCdnSecurityPolicy",
      "description": "Deletes an existing security policy within profile.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Cdn/profiles/{profileName}/securityPolicies/{securityPolicyName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.cdn/remove-azfrontdoorcdnsecuritypolicy"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ProfileName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Deletes an existing security policy within profile.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.securityPolicyName]"
            },
            {
              "name": "-ProfileName",
              "value": "[Path.profileName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
