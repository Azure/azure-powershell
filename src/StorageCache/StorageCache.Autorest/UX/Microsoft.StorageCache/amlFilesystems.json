{
  "resourceType": "amlFilesystems",
  "apiVersion": "2025-07-01",
  "learnMore": {
    "url": "https://learn.microsoft.com/powershell/module/az.storagecache"
  },
  "commands": [
    {
      "name": "Get-AzStorageCacheAmlFileSystem",
      "description": "Returns an AML file system.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StorageCache/amlFilesystems/{amlFilesystemName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagecache/get-azstoragecacheamlfilesystem"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String[]>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Returns an AML file system.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.amlFilesystemName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Remove-AzStorageCacheAmlFileSystem",
      "description": "Schedules an AML file system for deletion.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StorageCache/amlFilesystems/{amlFilesystemName}",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagecache/remove-azstoragecacheamlfilesystem"
        },
        "parameterSets": [
          {
            "parameters": [
              "-Name <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Schedules an AML file system for deletion.",
          "parameters": [
            {
              "name": "-Name",
              "value": "[Path.amlFilesystemName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
      "name": "Stop-AzStorageCacheAmlFilesystemArchive",
      "description": "Cancel archiving data from the AML file system.",
      "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.StorageCache/amlFilesystems/{amlFilesystemName}/cancelArchive",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagecache/stop-azstoragecacheamlfilesystemarchive"
        },
        "parameterSets": [
          {
            "parameters": [
              "-AmlFilesystemName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Cancel archiving data from the AML file system.",
          "parameters": [
            {
              "name": "-AmlFilesystemName",
              "value": "[Path.amlFilesystemName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    }
  ]
}
