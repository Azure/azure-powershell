  "commands": [
    {
      "name": "Clear-AzStorageCacheCach",
      "description": "Tells a cache to write all dirty data to the Storage Target(s).\nDuring the flush, clients will see errors returned until the flush is complete.",
      "path": "/subscriptions/{subscriptionId}/resourcegroups/{resourceGroupName}/providers/Microsoft.StorageCache/caches/{cacheName}/flush",
      "help": {
        "learnMore": {
          "url": "https://learn.microsoft.com/powershell/module/az.storagecache/clear-azstoragecachecach"
        },
        "parameterSets": [
          {
            "parameters": [
              "-EName <String>",
              "-ResourceGroupName <String>",
              "[-SubscriptionId <String>]"
            ]
          }
        ]
      },
      "examples": [
        {
          "description": "Tells a cache to write all dirty data to the Storage Target(s). During the flush, clients will see errors returned until the flush is complete.",
          "parameters": [
            {
              "name": "-EName",
              "value": "[Path.cacheName]"
            },
            {
              "name": "-ResourceGroupName",
              "value": "[Path.resourceGroupName]"
            },
            {
              "name": "-SubscriptionId",
              "value": "[Path.subscriptionId]"
            }
          ]
        }
      ]
    },
    {
