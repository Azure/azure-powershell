{
    "description":"This is a schema for data model of a resource type.",
    "type":"object",
    "properties":{
       "resourceType":{
          "type":"string"
       },
       "apiVersion":{
          "type":"string"
       },
       "learnMore":{
          "type":"object",
          "properties":{
             "url":{
                "type":"string"
             }
          },
          "required":[
             "url"
          ]
       },
       "commands":{
          "type":"array",
          "items":{
             "type":"object",
             "properties":{
                "name":{
                   "type":"string"
                },
                "description":{
                   "type":"string"
                },
                "path":{
                   "type":"string"
                },
                "confirmation":{
                   "type":"boolean"
                },
                "help":{
                   "type":"object",
                   "properties":{
                      "learnMore":{
                         "type":"object",
                         "properties":{
                            "url":{
                               "type":"string"
                            }
                         },
                         "required":[
                            "url"
                         ]
                      },
                      "parameterSets":{
                         "type":"array",
                         "items":{
                            "type":"object",
                            "properties":{
                               "name":{
                                  "type":"string"
                               },
                               "description":{
                                  "type":"string"
                               },
                               "parameters":{
                                  "type":"array",
                                  "items":{
                                     "type":"string"
                                  }
                               }
                            },
                            "required":["parameters"]
                         }
                      }
                   },
                   "required":[
                      "learnMore",
                      "parameterSets"
                   ]
                },
                "examples":{
                   "type":"array",
                   "items":{
                      "type":"object",
                      "properties":{
                         "description":{
                            "type":"string"
                         },
                         "parameters":{
                            "type":"array",
                            "items":{
                               "type":"object",
                               "properties":{
                                  "name":{
                                     "type":"string"
                                  },
                                  "value":{
                                     "type":"string"
                                  }
                               },
                               "required":[
                                  "name"
                               ]
                            }
                         }
                      },
                      "required":[
                         "description",
                         "parameters"
                      ]
                   }
                }
             },
             "required":[
                "name",
                "description",
                "path",
                "help",
                "examples"
             ]
          }
       }
    },
    "required":[
       "resourceType",
       "apiVersion",
       "learnMore",
       "commands"
    ],
    "additionalProperties":false
 }